                "200890002": {
                    "Id": 200890002,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": null,
                    "Name": null,
                    "IconPath": null,
                    "MainText": "我去找过若月老板占卜，她太准了",
                    "OptionText": "我去过，她太准了",
                    "NextItemIDList": [
                        200890003
                    ],
                    "Sender": "Player"
