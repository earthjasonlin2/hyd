                "200890007": {
                    "Id": 200890007,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": null,
                    "Name": null,
                    "IconPath": null,
                    "MainText": "什么「鉴爻轩」，我没去过，也不相信占卜",
                    "OptionText": "没去过，不信这个",
                    "NextItemIDList": [
                        200890008
                    ],
                    "Sender": "Player"
                },
                "200890003": {
                    "Id": 200890003,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": null,
                    "Name": null,
                    "IconPath": null,
