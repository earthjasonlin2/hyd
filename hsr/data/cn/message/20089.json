                "200890004": {
                    "Id": 200890004,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": null,
                    "Name": null,
                    "IconPath": null,
                    "MainText": "我去找过若月老板占卜，不太行，占卜结果一点都不准",
                    "OptionText": "我去过，不太靠谱",
                    "NextItemIDList": [
                        200890005
                    ],
                    "Sender": "Player"
