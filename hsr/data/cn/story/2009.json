{
    "Id": 2009,
    "Name": "巧言如流",
    "Buff": {
        "Name": "怪诞逸闻",
        "Desc": "我方目标击破敌方目标的弱点时会为【怪诞逸闻】充能。当充能达到<color=#f29e38ff><unbreak>#1[i]</unbreak></color>点时，对随机敌方目标造成<color=#f29e38ff><unbreak>#2[i]</unbreak></color>次固定数值的伤害。",
        "Param": [
            6,
            10,
            0.45000000041909516
        ]
    },
    "Option": [
        {
            "Name": "咏怀",
            "Desc": "敌方目标受到的击破伤害提高<color=#f29e38ff><unbreak>#1[i]%</unbreak></color>，我方攻击后若对敌方造成超击破伤害，则额外为【怪诞逸闻】充能<color=#f29e38ff><unbreak>#2[i]</unbreak></color>点。",
            "Param": [
                0.10000000009313226,
                1
            ]
        },
        {
            "Name": "画境",
            "Desc": "追加攻击造成的伤害提高<color=#f29e38ff><unbreak>#1[i]%</unbreak></color>，追加攻击的弱点击破效率提高<color=#f29e38ff><unbreak>#2[i]%</unbreak></color>。",
            "Param": [
                0.5000000004656613,
                0.5000000004656613
            ]
        },
        {
            "Name": "漫笔",
            "Desc": "角色施放终结技进行攻击时，无视敌方目标的弱点造成韧性削减。",
            "Param": []
        }
    ],
