                "127020001": {
                    "Id": 127020001,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": 63,
                    "Name": "Hex Club",
                    "IconPath": "SpriteOutput/AvatarRoundIcon/UI_Message_Contacts_Anonymous.png",
                    "MainText": "Congratulations on completing all Hexanexus in the beta!",
                    "OptionText": null,
                    "NextItemIDList": [
                        127020002
                    ],
                    "Sender": "NPC"
                },
                "127020002": {
                    "Id": 127020002,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": 63,
                    "Name": "Hex Club",
                    "IconPath": "SpriteOutput/AvatarRoundIcon/UI_Message_Contacts_Anonymous.png",
                    "MainText": "You completed all the Hexanexus stages. You must find the Hexanexus engaging!",
                    "OptionText": null,
                    "NextItemIDList": [
                        127020003,
                        127020004,
                        127020005
                    ],
                    "Sender": "NPC"
                },
                "127020003": {
                    "Id": 127020003,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": null,
                    "Name": null,
                    "IconPath": null,
                    "MainText": "Of course it's engaging. That's why I continued playing!",
                    "OptionText": "Super fun",
                    "NextItemIDList": [
                        127020006
                    ],
                    "Sender": "Player"
                },
                "127020004": {
                    "Id": 127020004,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": null,
                    "Name": null,
                    "IconPath": null,
                    "MainText": "I wanted to stop, but the treasures you give are too enticing",
                    "OptionText": "Too many goodies in the chest",
                    "NextItemIDList": [
                        127020006
                    ],
                    "Sender": "Player"
                },
                "127020005": {
                    "Id": 127020005,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": null,
                    "Name": null,
                    "IconPath": null,
                    "MainText": "If I don't continue, aren't you going to report me at the Realm-Keeping Commission? I'm scared",
                    "OptionText": "Weren't you going to sue me?",
                    "NextItemIDList": [
                        127020006
                    ],
                    "Sender": "Player"
                },
                "127020006": {
                    "Id": 127020006,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": 63,
                    "Name": "Hex Club",
                    "IconPath": "SpriteOutput/AvatarRoundIcon/UI_Message_Contacts_Anonymous.png",
                    "MainText": "If I may be honest with you, according to the Hex Club's prediction, a lot of people will fall in love and be obsessed with our Hexanexus",
                    "OptionText": null,
                    "NextItemIDList": [
                        127020007
                    ],
                    "Sender": "NPC"
                },
                "127020007": {
                    "Id": 127020007,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": 63,
                    "Name": "Hex Club",
                    "IconPath": "SpriteOutput/AvatarRoundIcon/UI_Message_Contacts_Anonymous.png",
                    "MainText": "But from the current statistics, only you have been able to complete all the challenges. The success rate is very low",
                    "OptionText": null,
                    "NextItemIDList": [
                        127020008,
                        127020010,
                        127020013
                    ],
                    "Sender": "NPC"
                },
                "127020008": {
                    "Id": 127020008,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": null,
                    "Name": null,
                    "IconPath": null,
                    "MainText": "I'm the only one who completed all the stages? But your Hexanexus is so interesting. I don't believe you",
                    "OptionText": "What happened?",
                    "NextItemIDList": [
                        127020009
                    ],
                    "Sender": "Player"
                },
                "127020010": {
                    "Id": 127020010,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": null,
                    "Name": null,
                    "IconPath": null,
                    "MainText": "Aren't you proficient in blackmailing? You can always report them. I'm not the only one you blackmailed, am I?",
                    "OptionText": "Go sue them at the Realm-Keeping Commission",
                    "NextItemIDList": [
                        127020011
                    ],
                    "Sender": "Player"
                },
                "127020013": {
                    "Id": 127020013,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": null,
                    "Name": null,
                    "IconPath": null,
                    "MainText": "WHAT!? So I'm the only one who spent so much time completing all the challenges? I've been such a fool",
                    "OptionText": "I'm the unluckiest loser ever",
                    "NextItemIDList": [
                        127020014
                    ],
                    "Sender": "Player"
                },
                "127020009": {
                    "Id": 127020009,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": 63,
                    "Name": "Hex Club",
                    "IconPath": "SpriteOutput/AvatarRoundIcon/UI_Message_Contacts_Anonymous.png",
                    "MainText": "You're so nice",
                    "OptionText": null,
                    "NextItemIDList": [
                        127020016
                    ],
                    "Sender": "NPC"
                },
                "127020011": {
                    "Id": 127020011,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": 63,
                    "Name": "Hex Club",
                    "IconPath": "SpriteOutput/AvatarRoundIcon/UI_Message_Contacts_Anonymous.png",
                    "MainText": "The Hex Club treats all friends who successfully retrieve the treasures in the same way",
                    "OptionText": null,
                    "NextItemIDList": [
                        127020012
                    ],
                    "Sender": "NPC"
                },
                "127020014": {
                    "Id": 127020014,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": 63,
                    "Name": "Hex Club",
                    "IconPath": "SpriteOutput/AvatarRoundIcon/UI_Message_Contacts_Anonymous.png",
                    "MainText": "It wasn't all that bad. We promised to give you the treasures as long as you keep playing",
                    "OptionText": null,
                    "NextItemIDList": [
                        127020015
                    ],
                    "Sender": "NPC"
                },
                "127020016": {
                    "Id": 127020016,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": 63,
                    "Name": "Hex Club",
                    "IconPath": "SpriteOutput/AvatarRoundIcon/UI_Message_Contacts_Anonymous.png",
                    "MainText": "Anyway, we hope to have a meeting with you. The intent of this meeting is: 1. To award you the Lifetime Hex Club Membership, and 2. to explore in-depth about your user experience and get some feedback",
                    "OptionText": null,
                    "NextItemIDList": [
                        127020017,
                        127020019,
                        127020022
                    ],
                    "Sender": "NPC"
                },
                "127020012": {
                    "Id": 127020012,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": 63,
                    "Name": "Hex Club",
                    "IconPath": "SpriteOutput/AvatarRoundIcon/UI_Message_Contacts_Anonymous.png",
                    "MainText": "Although, none of them had taken heed of the warning from the Hex Club",
                    "OptionText": null,
                    "NextItemIDList": [
                        127020016
                    ],
                    "Sender": "NPC"
                },
                "127020015": {
                    "Id": 127020015,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": 63,
                    "Name": "Hex Club",
                    "IconPath": "SpriteOutput/AvatarRoundIcon/UI_Message_Contacts_Anonymous.png",
                    "MainText": "And we did keep the promise. You took all the treasures from the challenge",
                    "OptionText": null,
                    "NextItemIDList": [
                        127020016
                    ],
                    "Sender": "NPC"
                },
                "127020017": {
                    "Id": 127020017,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": null,
                    "Name": null,
                    "IconPath": null,
                    "MainText": "Not really interested in the lifetime membership. Maybe skip that",
                    "OptionText": "Forget about titles",
                    "NextItemIDList": [
                        127020018
                    ],
                    "Sender": "Player"
                },
                "127020019": {
                    "Id": 127020019,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": null,
                    "Name": null,
                    "IconPath": null,
                    "MainText": "But I already filled in the survey for you",
                    "OptionText": "I've answered the survey already",
                    "NextItemIDList": [
                        127020020
                    ],
                    "Sender": "Player"
                },
                "127020022": {
                    "Id": 127020022,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": null,
                    "Name": null,
                    "IconPath": null,
                    "MainText": "Just tell me where are we meeting",
                    "OptionText": "Okay, where do we meet",
                    "NextItemIDList": [
                        127020023
                    ],
                    "Sender": "Player"
                },
