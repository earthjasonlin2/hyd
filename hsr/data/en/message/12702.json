                    "ItemContentID": null,
                    "ContactsID": 63,
                    "Name": "Hex Club",
                    "IconPath": "SpriteOutput/AvatarRoundIcon/UI_Message_Contacts_Anonymous.png",
                    "MainText": "Aside from the title, we also prepared a reward for the first world clear of the game. Your loss if you miss it",
                    "OptionText": null,
                    "NextItemIDList": [
                        127020023
                    ],
                    "Sender": "NPC"
                },
