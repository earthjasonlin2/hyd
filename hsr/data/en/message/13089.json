                },
                "130890008": {
                    "Id": 130890008,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": 106,
                    "Name": "Dr. Edward",
                    "IconPath": "SpriteOutput/AvatarRoundIcon/UI_Message_Contacts_DreamStore.png",
                    "MainText": "My store will be launching several thrilling dream bubbles. We're tentatively calling it the \"Apocalyptic Shadow\" series.",
                    "OptionText": null,
                    "NextItemIDList": [
                        130890009
                    ],
                    "Sender": "NPC"
                },
                "130890009": {
                    "Id": 130890009,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": 106,
                    "Name": "Dr. Edward",
                    "IconPath": "SpriteOutput/AvatarRoundIcon/UI_Message_Contacts_DreamStore.png",
                    "MainText": "The selling point of this series is to place the user in the middle of an apocalypse and have them personally feel the despair of watching on helplessly as the world collapses.",
                    "OptionText": null,
                    "NextItemIDList": [
                        130890010
                    ],
                    "Sender": "NPC"
                },
                "130890010": {
                    "Id": 130890010,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": null,
                    "Name": null,
                    "IconPath": null,
                    "MainText": "Despair? That's the selling point? Who would want that?",
                    "OptionText": "Who would want to feel despair?",
                    "NextItemIDList": [
                        130890011
                    ],
                    "Sender": "Player"
                },
                "130890011": {
                    "Id": 130890011,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": 106,
                    "Name": "Dr. Edward",
                    "IconPath": "SpriteOutput/AvatarRoundIcon/UI_Message_Contacts_DreamStore.png",
                    "MainText": "You may not know this, but being in a happy and harmonious environment for too long can numb one's spirit, and soon, nothing will be able to evoke joy anymore. This is where we come in â€” to offer a small stimulus of sorts.",
                    "OptionText": null,
                    "NextItemIDList": [
                        130890012
                    ],
                    "Sender": "NPC"
                },
                "130890012": {
                    "Id": 130890012,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": 106,
                    "Name": "Dr. Edward",
                    "IconPath": "SpriteOutput/AvatarRoundIcon/UI_Message_Contacts_DreamStore.png",
                    "MainText": "My secret partner hopes that you, dear {F#Ms.}{M#Mr.} {NICKNAME}, can help to give her a little push in this.",
                    "OptionText": null,
                    "NextItemIDList": [
                        130890013
                    ],
                    "Sender": "NPC"
                },
                "130890013": {
                    "Id": 130890013,
                    "ItemType": "Text",
                    "ItemContentID": null,
