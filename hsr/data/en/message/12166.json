                    "Id": 121660014,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": null,
                    "Name": null,
                    "IconPath": null,
                    "MainText": "To achieve the level of a great poet overnight is a monumental task...",
                    "OptionText": "This might be a foolhardy dream...",
                    "NextItemIDList": [
                        121660015
                    ],
                    "Sender": "Player"
