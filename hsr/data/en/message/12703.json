                    "Id": 127030014,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": 63,
                    "Name": "Hex Club",
                    "IconPath": "SpriteOutput/AvatarRoundIcon/UI_Message_Contacts_Anonymous.png",
                    "MainText": "I think that's a good one",
                    "OptionText": null,
