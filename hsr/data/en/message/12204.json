            "Messages": {
                "122040000": {
                    "Id": 122040000,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": 111,
                    "Name": "A-Yue",
                    "IconPath": "SpriteOutput/AvatarRoundIcon/UI_Message_Contacts_Anonymous.png",
                    "MainText": "Well, turns out this whole thing was just a misunderstanding. I'm really sorry about that...",
                    "OptionText": null,
                    "NextItemIDList": [
                        122040001
                    ],
                    "Sender": "NPC"
                },
                "122040001": {
                    "Id": 122040001,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": 111,
                    "Name": "A-Yue",
                    "IconPath": "SpriteOutput/AvatarRoundIcon/UI_Message_Contacts_Anonymous.png",
                    "MainText": "But I never thought I'd ever see my sister again after all these years.",
                    "OptionText": null,
                    "NextItemIDList": [
                        122040002,
                        122040003
                    ],
                    "Sender": "NPC"
                },
                "122040002": {
                    "Id": 122040002,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": null,
                    "Name": null,
                    "IconPath": null,
                    "MainText": "Umm, so what's the full story between you two? Care to tell me more about it?",
                    "OptionText": "I need more details.",
                    "NextItemIDList": [
                        122040004
                    ],
                    "Sender": "Player"
                },
                "122040003": {
                    "Id": 122040003,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": null,
                    "Name": null,
                    "IconPath": null,
                    "MainText": "Wait, hold on. What's happening here? Can you give me some context first?",
                    "OptionText": "Can I have some context?",
                    "NextItemIDList": [
                        122040004
                    ],
                    "Sender": "Player"
                },
                "122040004": {
                    "Id": 122040004,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": 1210,
                    "Name": "Guinaifen",
                    "IconPath": "SpriteOutput/AvatarRoundIcon/Avatar/1210.png",
                    "MainText": "Same here.",
                    "OptionText": null,
                    "NextItemIDList": [
                        122040005
                    ],
                    "Sender": "NPC"
                },
                "122040005": {
                    "Id": 122040005,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": 1206,
                    "Name": "Sushang",
                    "IconPath": "SpriteOutput/AvatarRoundIcon/Avatar/1206.png",
                    "MainText": "Same here.",
                    "OptionText": null,
                    "NextItemIDList": [
                        122040006
                    ],
                    "Sender": "NPC"
                },
                "122040006": {
                    "Id": 122040006,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": 111,
                    "Name": "A-Yue",
                    "IconPath": "SpriteOutput/AvatarRoundIcon/UI_Message_Contacts_Anonymous.png",
                    "MainText": "My parents were Cloud Knights, but they passed away when we were young.",
                    "OptionText": null,
                    "NextItemIDList": [
                        122040007
                    ],
                    "Sender": "NPC"
                },
                "122040007": {
                    "Id": 122040007,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": 111,
                    "Name": "A-Yue",
                    "IconPath": "SpriteOutput/AvatarRoundIcon/UI_Message_Contacts_Anonymous.png",
                    "MainText": "Since I had to support both of us, I had to work all day and didn't have much time to discipline my sister.",
                    "OptionText": null,
                    "NextItemIDList": [
                        122040008
                    ],
                    "Sender": "NPC"
                },
                "122040008": {
                    "Id": 122040008,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": 111,
                    "Name": "A-Yue",
                    "IconPath": "SpriteOutput/AvatarRoundIcon/UI_Message_Contacts_Anonymous.png",
                    "MainText": "As a result, she ended up becoming a troublemaker and didn't even finish school.",
                    "OptionText": null,
                    "NextItemIDList": [
                        122040009
                    ],
                    "Sender": "NPC"
                },
                "122040009": {
                    "Id": 122040009,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": 111,
                    "Name": "A-Yue",
                    "IconPath": "SpriteOutput/AvatarRoundIcon/UI_Message_Contacts_Anonymous.png",
                    "MainText": "So, I secretly applied for her to join the Cloud Knights, hoping that being in the forces would straighten her out... and maybe she'd become a better person.",
                    "OptionText": null,
                    "NextItemIDList": [
                        122040010
                    ],
                    "Sender": "NPC"
                },
                "122040010": {
                    "Id": 122040010,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": 111,
                    "Name": "A-Yue",
                    "IconPath": "SpriteOutput/AvatarRoundIcon/UI_Message_Contacts_Anonymous.png",
                    "MainText": "When she found out what I did, we had a huge argument. She thought I was meddling in her life.",
                    "OptionText": null,
                    "NextItemIDList": [
                        122040011
                    ],
                    "Sender": "NPC"
                },
                "122040011": {
                    "Id": 122040011,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": 111,
                    "Name": "A-Yue",
                    "IconPath": "SpriteOutput/AvatarRoundIcon/UI_Message_Contacts_Anonymous.png",
                    "MainText": "I thought she'd refuse to serve and spill the beans to the Military Affairs Office, but... she actually joined the Cloud Knights.",
                    "OptionText": null,
                    "NextItemIDList": [
                        122040012
                    ],
                    "Sender": "NPC"
                },
                "122040012": {
                    "Id": 122040012,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": 111,
                    "Name": "A-Yue",
                    "IconPath": "SpriteOutput/AvatarRoundIcon/UI_Message_Contacts_Anonymous.png",
                    "MainText": "After that, we started to drift apart and eventually, one day, she disappeared without a trace.",
                    "OptionText": null,
                    "NextItemIDList": [
                        122040013,
                        122040014
                    ],
                    "Sender": "NPC"
                },
                "122040013": {
                    "Id": 122040013,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": null,
                    "Name": null,
                    "IconPath": null,
                    "MainText": "You did what you thought was best for her. She'll understand it when she grows up. Don't blame yourself over it.",
                    "OptionText": "You did what was best for her.",
                    "NextItemIDList": [
                        122040015
                    ],
                    "Sender": "Player"
                },
                "122040014": {
                    "Id": 122040014,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": null,
                    "Name": null,
                    "IconPath": null,
                    "MainText": "It's good for unruly youngsters to go through some tough experiences to shape them up.",
                    "OptionText": "Youths need some tempering.",
                    "NextItemIDList": [
                        122040015
                    ],
                    "Sender": "Player"
                },
                "122040015": {
                    "Id": 122040015,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": 111,
                    "Name": "A-Yue",
                    "IconPath": "SpriteOutput/AvatarRoundIcon/UI_Message_Contacts_Anonymous.png",
                    "MainText": "I always thought she didn't want to see me because she resented me for enlisting her in the Cloud Knights.",
                    "OptionText": null,
                    "NextItemIDList": [
                        122040016
                    ],
                    "Sender": "NPC"
                },
                "122040016": {
                    "Id": 122040016,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": 111,
                    "Name": "A-Yue",
                    "IconPath": "SpriteOutput/AvatarRoundIcon/UI_Message_Contacts_Anonymous.png",
                    "MainText": "But when I saw her today, she told me she got transferred to the Ten-Lords Commission and didn't have the chance to contact me and explain everything.",
                    "OptionText": null,
                    "NextItemIDList": [
                        122040017
                    ],
                    "Sender": "NPC"
                },
                "122040017": {
                    "Id": 122040017,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": 111,
                    "Name": "A-Yue",
                    "IconPath": "SpriteOutput/AvatarRoundIcon/UI_Message_Contacts_Anonymous.png",
                    "MainText": "Now that I see she's doing well, I can finally breathe a sigh of relief. As long as she doesn't regret it, I'm okay with it.",
                    "OptionText": null,
                    "NextItemIDList": [
                        122040018
                    ],
                    "Sender": "NPC"
                },
                "122040018": {
                    "Id": 122040018,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": 111,
                    "Name": "A-Yue",
                    "IconPath": "SpriteOutput/AvatarRoundIcon/UI_Message_Contacts_Anonymous.png",
                    "MainText": "Anyway, it was a misunderstanding from the start. Sorry for dragging you into my family drama.",
                    "OptionText": null,
                    "NextItemIDList": [
                        122040019
                    ],
                    "Sender": "NPC"
                },
                "122040019": {
                    "Id": 122040019,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": 111,
                    "Name": "A-Yue",
                    "IconPath": "SpriteOutput/AvatarRoundIcon/UI_Message_Contacts_Anonymous.png",
                    "MainText": "Go for it, Little Gui! I'll be supporting you!",
                    "OptionText": null,
                    "NextItemIDList": [
                        122040020
                    ],
                    "Sender": "NPC"
                },
