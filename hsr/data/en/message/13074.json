                "130740000": {
                    "Id": 130740000,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": 146,
                    "Name": "Clock Studios Customer Service",
                    "IconPath": "SpriteOutput/AvatarRoundIcon/UI_Message_Contacts_Anonymous.png",
                    "MainText": "Dear visitor, according to the Bloodhound Family, Clock Studios Theme Park is currently undergoing a security check, so the main gate is temporarily closed.",
                    "OptionText": null,
                    "NextItemIDList": [
                        130740001
                    ],
                    "Sender": "NPC"
                },
                "130740001": {
                    "Id": 130740001,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": 146,
                    "Name": "Clock Studios Customer Service",
                    "IconPath": "SpriteOutput/AvatarRoundIcon/UI_Message_Contacts_Anonymous.png",
                    "MainText": "However, to satisfy our customers, we will continue providing the amusement program, \"<i>Clockie's Extras</i> Filming Experience,\" where you can enjoy fun games and learn about the creation of the animations <i>Hamster Ball Knight</i> and <i>Brother Hanu</i>.",
                    "OptionText": null,
                    "NextItemIDList": [
                        130740002,
                        130740006,
                        130740010
                    ],
                    "Sender": "NPC"
                },
                "130740002": {
                    "Id": 130740002,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": null,
                    "Name": null,
                    "IconPath": null,
                    "MainText": "Didn't you just say the park is closed?",
                    "OptionText": "Didn't you just say the park is closed?",
                    "NextItemIDList": [
                        130740003
                    ],
                    "Sender": "Player"
                },
                "130740006": {
                    "Id": 130740006,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": null,
                    "Name": null,
                    "IconPath": null,
                    "MainText": "Why do you recommend this?",
                    "OptionText": "Why are you recommending this program?",
                    "NextItemIDList": [
                        130740007
                    ],
                    "Sender": "Player"
                },
                "130740010": {
                    "Id": 130740010,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": null,
                    "Name": null,
                    "IconPath": null,
                    "MainText": "I'm not interested.",
                    "OptionText": "I'd like to try something else.",
                    "NextItemIDList": [
                        130740011
                    ],
                    "Sender": "Player"
                },
                "130740003": {
                    "Id": 130740003,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": null,
                    "Name": null,
                    "IconPath": null,
                    "MainText": "Why can we still participate in the program?",
                    "OptionText": null,
                    "NextItemIDList": [
                        130740004
                    ],
                    "Sender": "PlayerAuto"
                },
                "130740007": {
                    "Id": 130740007,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": null,
                    "Name": null,
                    "IconPath": null,
                    "MainText": "Is it because the other programs aren't fun enough?",
                    "OptionText": null,
                    "NextItemIDList": [
                        130740008
                    ],
                    "Sender": "PlayerAuto"
                },
                "130740011": {
                    "Id": 130740011,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": null,
                    "Name": null,
                    "IconPath": null,
                    "MainText": "I want to play something else.",
                    "OptionText": null,
                    "NextItemIDList": [
                        130740012
                    ],
                    "Sender": "PlayerAuto"
                },
                "130740004": {
                    "Id": 130740004,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": 146,
                    "Name": "Clock Studios Customer Service",
                    "IconPath": "SpriteOutput/AvatarRoundIcon/UI_Message_Contacts_Anonymous.png",
                    "MainText": "As you may be aware, Clock Studios was not founded by The Family.",
                    "OptionText": null,
                    "NextItemIDList": [
                        130740005
                    ],
                    "Sender": "NPC"
                },
                "130740008": {
                    "Id": 130740008,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": 146,
                    "Name": "Clock Studios Customer Service",
                    "IconPath": "SpriteOutput/AvatarRoundIcon/UI_Message_Contacts_Anonymous.png",
                    "MainText": "Because this is our flagship program and has been the highest-rated one at the park for years.",
                    "OptionText": null,
                    "NextItemIDList": [
                        130740009
                    ],
                    "Sender": "NPC"
                },
                "130740012": {
                    "Id": 130740012,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": 146,
                    "Name": "Clock Studios Customer Service",
                    "IconPath": "SpriteOutput/AvatarRoundIcon/UI_Message_Contacts_Anonymous.png",
                    "MainText": "You are free to choose, of course. However, please be aware that if you encounter an accident in another program, Clock Studios Theme Park will not be held responsible.",
                    "OptionText": null,
                    "NextItemIDList": [
                        130740013
                    ],
                    "Sender": "NPC"
                },
                "130740005": {
                    "Id": 130740005,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": 146,
                    "Name": "Clock Studios Customer Service",
                    "IconPath": "SpriteOutput/AvatarRoundIcon/UI_Message_Contacts_Anonymous.png",
                    "MainText": "We will operate based on our Chairman, the Watchmaker's, philosophy. We won't compromise our own interests for The Family.",
                    "OptionText": null,
                    "NextItemIDList": [
                        130740013
                    ],
                    "Sender": "NPC"
                },
                "130740009": {
                    "Id": 130740009,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": 146,
                    "Name": "Clock Studios Customer Service",
                    "IconPath": "SpriteOutput/AvatarRoundIcon/UI_Message_Contacts_Anonymous.png",
                    "MainText": "But of course, there are other reasons, but let's not get into that right now.",
                    "OptionText": null,
                    "NextItemIDList": [
                        130740013
                    ],
                    "Sender": "NPC"
                },
                "130740013": {
                    "Id": 130740013,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": 146,
                    "Name": "Clock Studios Customer Service",
                    "IconPath": "SpriteOutput/AvatarRoundIcon/UI_Message_Contacts_Anonymous.png",
                    "MainText": "For your information, you can create your own animation in this program, and if your work is good enough, it might even get officially released in theaters!",
                    "OptionText": null,
                    "NextItemIDList": [
                        130740014
                    ],
                    "Sender": "NPC"
                },
                "130740014": {
                    "Id": 130740014,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": 146,
                    "Name": "Clock Studios Customer Service",
                    "IconPath": "SpriteOutput/AvatarRoundIcon/UI_Message_Contacts_Anonymous.png",
                    "MainText": "As a reminder to you, the ownership and intellectual property rights of the animation you produce using Clock Studios' facilities, along with related rights and interests, belong to Clock Studios. Clock Studios has the right to retain and dispose of the content, and any revenue generated from the content will belong to Clock Studios.",
                    "OptionText": null,
                    "NextItemIDList": [
                        130740015,
                        130740018,
                        130740022
                    ],
                    "Sender": "NPC"
                },
                "130740015": {
                    "Id": 130740015,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": null,
                    "Name": null,
                    "IconPath": null,
                    "MainText": "These terms sound weird.",
                    "OptionText": "Why do these terms sound so weird...?",
                    "NextItemIDList": [
                        130740016
                    ],
                    "Sender": "Player"
                },
                "130740018": {
                    "Id": 130740018,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": null,
                    "Name": null,
                    "IconPath": null,
                    "MainText": "So, everything I create belongs to you?",
                    "OptionText": "Trying to make me work for free?",
                    "NextItemIDList": [
                        130740019
                    ],
                    "Sender": "Player"
                },
                "130740022": {
                    "Id": 130740022,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": null,
                    "Name": null,
                    "IconPath": null,
                    "MainText": "Too long. Didn't understand a thing.",
                    "OptionText": "I don't understand a word.",
                    "NextItemIDList": [
                        130740023
                    ],
                    "Sender": "Player"
                },
                "130740016": {
                    "Id": 130740016,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": null,
                    "Name": null,
                    "IconPath": null,
                    "MainText": "Is this some kind of scam?",
                    "OptionText": null,
                    "NextItemIDList": [
                        130740017
                    ],
                    "Sender": "PlayerAuto"
                },
                "130740019": {
                    "Id": 130740019,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": null,
                    "Name": null,
                    "IconPath": null,
                    "MainText": "Is this idea to help your animation team slack off?",
                    "OptionText": null,
                    "NextItemIDList": [
                        130740020
                    ],
                    "Sender": "PlayerAuto"
                },
                "130740023": {
                    "Id": 130740023,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": 146,
                    "Name": "Clock Studios Customer Service",
                    "IconPath": "SpriteOutput/AvatarRoundIcon/UI_Message_Contacts_Anonymous.png",
                    "MainText": "Sorry, but compared to copyright protection terms elsewhere in the universe, like those of Interastral Peace Entertainment, our terms are already quite concise and clear. Their interactive projects often require tens of thousands of words to be displayed before playing.",
                    "OptionText": null,
                    "NextItemIDList": [
                        130740024
                    ],
                    "Sender": "NPC"
                },
                "130740017": {
                    "Id": 130740017,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": 146,
                    "Name": "Clock Studios Customer Service",
                    "IconPath": "SpriteOutput/AvatarRoundIcon/UI_Message_Contacts_Anonymous.png",
                    "MainText": "Please don't worry. These are merely measures for copyright protection.",
                    "OptionText": null,
                    "NextItemIDList": [
                        130740024
                    ],
                    "Sender": "NPC"
                },
                "130740020": {
                    "Id": 130740020,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": 146,
                    "Name": "Clock Studios Customer Service",
                    "IconPath": "SpriteOutput/AvatarRoundIcon/UI_Message_Contacts_Anonymous.png",
                    "MainText": "Sorry for the confusion. While we are delighted to share our animations with guests, we need to protect our studio's intellectual property.",
                    "OptionText": null,
                    "NextItemIDList": [
                        130740021
                    ],
                    "Sender": "NPC"
                },
                "130740024": {
                    "Id": 130740024,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": 146,
                    "Name": "Clock Studios Customer Service",
                    "IconPath": "SpriteOutput/AvatarRoundIcon/UI_Message_Contacts_Anonymous.png",
                    "MainText": "If you are interested, please find a yellow-haired Pepeshi employee at the park entrance, and he will guide you through the program.",
                    "OptionText": null,
                    "NextItemIDList": [
                        130740025
                    ],
                    "Sender": "NPC"
                },
                "130740021": {
                    "Id": 130740021,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": 146,
                    "Name": "Clock Studios Customer Service",
                    "IconPath": "SpriteOutput/AvatarRoundIcon/UI_Message_Contacts_Anonymous.png",
                    "MainText": "If your animation gets officially released in theaters, you'll get a <unbreak>1.5</unbreak>-second shout-out in the credits.",
                    "OptionText": null,
                    "NextItemIDList": [
                        130740024
                    ],
                    "Sender": "NPC"
                },
                "130740025": {
                    "Id": 130740025,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": 146,
                    "Name": "Clock Studios Customer Service",
                    "IconPath": "SpriteOutput/AvatarRoundIcon/UI_Message_Contacts_Anonymous.png",
                    "MainText": "Please remember the password for Dreamville. \"Hmhm! Charrrrrrge!\"",
                    "OptionText": null,
                    "NextItemIDList": [
                        130740026
                    ],
                    "Sender": "NPC"
                },
