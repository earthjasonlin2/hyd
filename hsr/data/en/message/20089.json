                "200890021": {
                    "Id": 200890021,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": null,
                    "Name": null,
                    "IconPath": null,
                    "MainText": "As the head of the Divination Commission, for me to go to a street side stall to get her fortunes read... wouldn't that be rather strange?",
                    "OptionText": null,
                    "NextItemIDList": [
                        200890022
                    ],
                    "Sender": "NPC"
                },
                "200890023": {
                    "Id": 200890023,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": null,
                    "Name": null,
                    "IconPath": null,
                    "MainText": "*sigh* It was I who personally drove her out of the Divination Commission.",
                    "OptionText": null,
                    "NextItemIDList": [
                        200890024
                    ],
                    "Sender": "NPC"
                },
                "200890022": {
                    "Id": 200890022,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": null,
                    "Name": null,
                    "IconPath": null,
                    "MainText": "Besides...",
                    "OptionText": null,
