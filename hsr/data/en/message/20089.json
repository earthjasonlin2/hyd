                "200890024": {
                    "Id": 200890024,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": null,
                    "Name": null,
                    "IconPath": null,
                    "MainText": "The hexagram from the Matrix of Prescience showed that Ruoyue will be driven out of the Divination Commission by my own hands. I fear that implied she might be in mortal danger if she stayed in the Commission.",
                    "OptionText": null,
                    "NextItemIDList": [
                        200890025
                    ],
                    "Sender": "NPC"
                },
                "200890025": {
                    "Id": 200890025,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": null,
                    "Name": null,
                    "IconPath": null,
                    "MainText": "Of course, I never told her this. I just told her that I divined this outcome between us.",
                    "OptionText": null,
                    "NextItemIDList": [
                        200890026,
                        200890033
                    ],
                    "Sender": "NPC"
                },
                "200890026": {
                    "Id": 200890026,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": null,
                    "Name": null,
                    "IconPath": null,
                    "MainText": "This is ridiculous",
