            "Messages": {
                "200890000": {
                    "Id": 200890000,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": null,
                    "Name": null,
                    "IconPath": null,
                    "MainText": "Do you know of The Looking Bronze in Exalting Sanctum? The divination shop ran by Ruoyue?",
                    "OptionText": null,
                    "NextItemIDList": [
                        200890001
                    ],
                    "Sender": "NPC"
                },
                "200890001": {
                    "Id": 200890001,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": null,
                    "Name": null,
                    "IconPath": null,
                    "MainText": "I heard from the other diviners that the shop is quite popular amongst outworlders. Have you been to it? What do you think?",
                    "OptionText": null,
                    "NextItemIDList": [
                        200890002,
                        200890004,
                        200890007
                    ],
                    "Sender": "NPC"
                },
                "200890002": {
                    "Id": 200890002,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": null,
                    "Name": null,
                    "IconPath": null,
                    "MainText": "Ruoyue did a reading for me. She's super accurate",
                    "OptionText": "I've been there. She's way too accurate",
                    "NextItemIDList": [
                        200890003
                    ],
                    "Sender": "Player"
                },
                "200890004": {
                    "Id": 200890004,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": null,
                    "Name": null,
                    "IconPath": null,
                    "MainText": "Ruoyue did a reading for me. It was mediocre. Wasn't accurate at all",
                    "OptionText": "I've been there. It's not that reliable.",
                    "NextItemIDList": [
                        200890005
                    ],
                    "Sender": "Player"
                },
                "200890007": {
                    "Id": 200890007,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": null,
                    "Name": null,
                    "IconPath": null,
                    "MainText": "The Looking Bronze? I've never been. I don't believe in divination",
                    "OptionText": "Never been there. Don't believe in it",
                    "NextItemIDList": [
                        200890008
                    ],
                    "Sender": "Player"
                },
                "200890003": {
                    "Id": 200890003,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": null,
                    "Name": null,
                    "IconPath": null,
                    "MainText": "As expected. After all, she was once a Divination Commission diviner.",
                    "OptionText": null,
                    "NextItemIDList": [
                        200890010
                    ],
                    "Sender": "NPC"
                },
                "200890005": {
                    "Id": 200890005,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": null,
                    "Name": null,
                    "IconPath": null,
                    "MainText": "Really? Would Ruoyue really trick her customers so, now that she's left the Divination Commission?",
                    "OptionText": null,
                    "NextItemIDList": [
                        200890006
                    ],
                    "Sender": "NPC"
                },
                "200890008": {
                    "Id": 200890008,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": null,
                    "Name": null,
                    "IconPath": null,
                    "MainText": "You have witnessed my power to divine the future firsthand, {NICKNAME}! You have some guts, saying that you don't believe in divination to the master diviner's face!",
                    "OptionText": null,
                    "NextItemIDList": [
                        200890009
                    ],
                    "Sender": "NPC"
                },
                "200890010": {
                    "Id": 200890010,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": null,
                    "Name": null,
                    "IconPath": null,
                    "MainText": "{NICKNAME}, I have a favor to ask of you.",
                    "OptionText": null,
                    "NextItemIDList": [
                        200890011
                    ],
                    "Sender": "NPC"
                },
                "200890006": {
                    "Id": 200890006,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": null,
                    "Name": null,
                    "IconPath": null,
                    "MainText": "Or did her skills grow rusty...",
                    "OptionText": null,
                    "NextItemIDList": [
                        200890010
                    ],
                    "Sender": "NPC"
                },
                "200890009": {
                    "Id": 200890009,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": null,
                    "Name": null,
                    "IconPath": null,
                    "MainText": "To be honest with you, Ruoyue's skills are nothing to scoff at amongst the former employees of the Commission.",
                    "OptionText": null,
                    "NextItemIDList": [
                        200890010
                    ],
                    "Sender": "NPC"
                },
                "200890011": {
                    "Id": 200890011,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": null,
                    "Name": null,
                    "IconPath": null,
                    "MainText": "Please go do a divination in Ruoyue's shop whenever you pass through Exalting Sanctum, so she will get some business.",
                    "OptionText": null,
                    "NextItemIDList": [
                        200890012
                    ],
                    "Sender": "NPC"
                },
                "200890012": {
                    "Id": 200890012,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": null,
                    "Name": null,
                    "IconPath": null,
                    "MainText": "...That's the least I can do as her former boss.",
                    "OptionText": null,
                    "NextItemIDList": [
                        200890013,
                        200890016,
                        200890019
                    ],
                    "Sender": "NPC"
                },
                "200890013": {
                    "Id": 200890013,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": null,
                    "Name": null,
                    "IconPath": null,
                    "MainText": "You want me to help your old employee's business?",
                    "OptionText": "Can I ask why?",
                    "NextItemIDList": [
                        200890014
                    ],
                    "Sender": "Player"
                },
                "200890016": {
                    "Id": 200890016,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": null,
                    "Name": null,
                    "IconPath": null,
                    "MainText": "You are rather empathetic today. I'm having a hard time adjusting to this version of Fu Xuan...",
                    "OptionText": "I'm not used to this Fu Xuan...",
                    "NextItemIDList": [
                        200890017
                    ],
                    "Sender": "Player"
                },
                "200890019": {
                    "Id": 200890019,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": null,
                    "Name": null,
                    "IconPath": null,
                    "MainText": "It won't be hard for you to personally help her business, right?",
                    "OptionText": "You can't go yourself?",
                    "NextItemIDList": [
                        200890020
                    ],
                    "Sender": "Player"
                },
                "200890014": {
                    "Id": 200890014,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": null,
                    "Name": null,
                    "IconPath": null,
                    "MainText": "Sure, I can do that... but can I ask why?",
                    "OptionText": null,
                    "NextItemIDList": [
                        200890015
                    ],
                    "Sender": "PlayerAuto"
                },
                "200890017": {
                    "Id": 200890017,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": null,
                    "Name": null,
                    "IconPath": null,
                    "MainText": "What, do you take me as some emotionless divination robot?",
                    "OptionText": null,
                    "NextItemIDList": [
                        200890018
                    ],
                    "Sender": "NPC"
                },
                "200890020": {
                    "Id": 200890020,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": null,
                    "Name": null,
                    "IconPath": null,
                    "MainText": "Put yourself into my shoes for a moment...",
                    "OptionText": null,
                    "NextItemIDList": [
                        200890021
                    ],
                    "Sender": "NPC"
                },
                "200890015": {
                    "Id": 200890015,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": null,
                    "Name": null,
                    "IconPath": null,
                    "MainText": "It's because...",
