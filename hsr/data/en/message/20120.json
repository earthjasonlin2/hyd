    "Sections": [
        {
            "Id": 2012000,
            "Messages": {
                "201200000": {
                    "Id": 201200000,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": null,
                    "Name": null,
                    "IconPath": null,
                    "MainText": "The esteemed University of Veritas Prime has once again commenced its enrollment proceedings. Do you, perchance, harbor aspirations for further refinements and scholarly pursuits?",
                    "OptionText": null,
                    "NextItemIDList": [
                        201200001
                    ],
                    "Sender": "NPC"
                },
                "201200001": {
                    "Id": 201200001,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": null,
                    "Name": null,
                    "IconPath": null,
                    "MainText": "Huh? Wdym",
                    "OptionText": "Huh?",
                    "NextItemIDList": [
                        201200002
                    ],
                    "Sender": "Player"
                },
                "201200002": {
                    "Id": 201200002,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": null,
                    "Name": null,
                    "IconPath": null,
                    "MainText": "Interstellar voyages necessitate a wealth of erudition, lest one may very well perish amidst perils unknown.",
                    "OptionText": null,
                    "NextItemIDList": [
                        201200003
                    ],
                    "Sender": "NPC"
                },
                "201200003": {
                    "Id": 201200003,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": null,
                    "Name": null,
                    "IconPath": null,
                    "MainText": "What subjects tickle your fancy? Mathematics, physics, or perhaps, philosophy...?",
                    "OptionText": null,
                    "NextItemIDList": [
                        201200004,
                        201200013,
                        201200018
                    ],
                    "Sender": "NPC"
                },
                "201200004": {
                    "Id": 201200004,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": null,
                    "Name": null,
                    "IconPath": null,
                    "MainText": "I like math?",
                    "OptionText": "I like math?",
                    "NextItemIDList": [
                        201200005
                    ],
                    "Sender": "Player"
                },
                "201200013": {
                    "Id": 201200013,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": null,
                    "Name": null,
                    "IconPath": null,
                    "MainText": "I like physics.",
                    "OptionText": "I like physics.",
                    "NextItemIDList": [
                        201200014
                    ],
                    "Sender": "Player"
                },
                "201200018": {
                    "Id": 201200018,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": null,
                    "Name": null,
                    "IconPath": null,
                    "MainText": "I like philosophy!",
                    "OptionText": "I like philosophy!",
                    "NextItemIDList": [
                        201200019
                    ],
                    "Sender": "Player"
                },
                "201200005": {
                    "Id": 201200005,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": null,
                    "Name": null,
                    "IconPath": null,
                    "MainText": "Allow me to regale you with a tale.",
                    "OptionText": null,
                    "NextItemIDList": [
                        201200006
                    ],
                    "Sender": "NPC"
                },
                "201200014": {
                    "Id": 201200014,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": null,
                    "Name": null,
                    "IconPath": null,
                    "MainText": "Nowadays, physics journals are being published at a rate surpassing even the speed of light.",
                    "OptionText": null,
                    "NextItemIDList": [
                        201200015
                    ],
                    "Sender": "NPC"
                },
                "201200019": {
                    "Id": 201200019,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": null,
                    "Name": null,
                    "IconPath": null,
                    "MainText": "Are you privy to the demarcation between philosophy and science?",
                    "OptionText": null,
                    "NextItemIDList": [
                        201200020
                    ],
                    "Sender": "NPC"
                },
                "201200006": {
                    "Id": 201200006,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": null,
                    "Name": null,
                    "IconPath": null,
                    "MainText": "Once upon a time, a physicist and an engineer found themselves adrift in a hot air balloon amidst a ravine.",
                    "OptionText": null,
                    "NextItemIDList": [
                        201200007
                    ],
                    "Sender": "NPC"
                },
                "201200015": {
                    "Id": 201200015,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": null,
                    "Name": null,
                    "IconPath": null,
                    "MainText": "This, however, does not defy the law of relativity, for all those journals carry not even an iota of meaningful information.",
                    "OptionText": null,
                    "NextItemIDList": [
                        201200016
                    ],
                    "Sender": "NPC"
                },
                "201200020": {
                    "Id": 201200020,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": null,
                    "Name": null,
                    "IconPath": null,
                    "MainText": "To tackle scientific quandaries, one requires parchment, quill, and an eraser.",
                    "OptionText": null,
                    "NextItemIDList": [
                        201200021
                    ],
                    "Sender": "NPC"
                },
                "201200007": {
                    "Id": 201200007,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": null,
                    "Name": null,
                    "IconPath": null,
                    "MainText": "They screamed at the top of their lungs, \"Yo-ho! Where are we?\"",
                    "OptionText": null,
                    "NextItemIDList": [
                        201200008
                    ],
                    "Sender": "NPC"
                },
                "201200016": {
                    "Id": 201200016,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": null,
                    "Name": null,
                    "IconPath": null,
                    "MainText": "Moreover, should you ever embark upon this line of academic endeavor, it is highly probable that a staggering four-fifths of the year shall be exclusively consumed by languid progression, while the remaining fifth shall be indubitably bereft of any semblance of progression at all.",
                    "OptionText": null,
                    "NextItemIDList": [
                        201200017
                    ],
                    "Sender": "NPC"
                },
                "201200021": {
                    "Id": 201200021,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": null,
                    "Name": null,
                    "IconPath": null,
                    "MainText": "Yet, to unravel philosophical enigmas, parchment and quill shall suffice.",
                    "OptionText": null,
                    "NextItemIDList": [
                        201200022
                    ],
                    "Sender": "NPC"
                },
                "201200008": {
                    "Id": 201200008,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": null,
                    "Name": null,
                    "IconPath": null,
                    "MainText": "After fifteen minutes had elapsed, they heard a voice reply, \"Yo-ho! You are in a hot air balloon!\"",
                    "OptionText": null,
                    "NextItemIDList": [
                        201200009
                    ],
                    "Sender": "NPC"
                },
                "201200017": {
                    "Id": 201200017,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": null,
                    "Name": null,
                    "IconPath": null,
                    "MainText": "Sounds pretty scary.",
                    "OptionText": "Sounds pretty scary.",
                    "NextItemIDList": [
                        201200025
                    ],
                    "Sender": "Player"
                },
                "201200022": {
                    "Id": 201200022,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": null,
                    "Name": null,
                    "IconPath": null,
                    "MainText": "For in philosophy lies a notable lack of a definitive criterion for right and wrong.",
                    "OptionText": null,
                    "NextItemIDList": [
                        201200023
                    ],
                    "Sender": "NPC"
                },
                "201200009": {
                    "Id": 201200009,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": null,
                    "Name": null,
                    "IconPath": null,
                    "MainText": "The physicist deduced, \"Surely, that must be a mathematician...\"",
                    "OptionText": null,
                    "NextItemIDList": [
                        201200010
                    ],
                    "Sender": "NPC"
                },
                "201200025": {
                    "Id": 201200025,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": null,
                    "Name": null,
                    "IconPath": null,
                    "MainText": "You may need more time for careful consideration.",
                    "OptionText": null,
                    "NextItemIDList": [
                        201200026
                    ],
                    "Sender": "NPC"
                },
                "201200023": {
                    "Id": 201200023,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": null,
                    "Name": null,
                    "IconPath": null,
                    "MainText": "I would not recommend this discipline to neophytes.",
                    "OptionText": null,
                    "NextItemIDList": [
                        201200024
                    ],
                    "Sender": "NPC"
                },
                "201200010": {
                    "Id": 201200010,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": null,
                    "Name": null,
                    "IconPath": null,
                    "MainText": "\"...for he took an extensive period of time to produce an entirely accurate answer, albeit utterly inconsequential.\"",
                    "OptionText": null,
                    "NextItemIDList": [
                        201200011
                    ],
                    "Sender": "NPC"
                },
                "201200026": {
                    "Id": 201200026,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": null,
                    "Name": null,
                    "IconPath": null,
                    "MainText": "Beware of those who only extol the virtues of an academic discipline without candidly acknowledging its challenges.",
                    "OptionText": null,
                    "NextItemIDList": [
                        201200027
                    ],
                    "Sender": "NPC"
                },
                "201200024": {
                    "Id": 201200024,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": null,
                    "Name": null,
                    "IconPath": null,
                    "MainText": "Understood",
                    "OptionText": "Understood",
                    "NextItemIDList": [
                        201200025
                    ],
                    "Sender": "Player"
                },
                "201200011": {
                    "Id": 201200011,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": null,
                    "Name": null,
                    "IconPath": null,
                    "MainText": "Truth be told, mathematics is a formidable discipline, one that eludes the grasp of many throughout their lifetime, rendering its significance inscrutable.",
                    "OptionText": null,
                    "NextItemIDList": [
                        201200012
                    ],
                    "Sender": "NPC"
                },
                "201200027": {
                    "Id": 201200027,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": null,
                    "Name": null,
                    "IconPath": null,
                    "MainText": "I have met numerous students brimming with enthusiasm, only to witness their eventual departure, ensconced in disillusionment and despondency.",
                    "OptionText": null,
                    "NextItemIDList": [
                        201200028
                    ],
                    "Sender": "NPC"
                },
                "201200012": {
                    "Id": 201200012,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": null,
                    "Name": null,
                    "IconPath": null,
                    "MainText": "What a dry joke that is.",
                    "OptionText": "What a dry joke that is.",
                    "NextItemIDList": [
                        201200025
                    ],
                    "Sender": "Player"
                },
                "201200028": {
                    "Id": 201200028,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": null,
                    "Name": null,
                    "IconPath": null,
                    "MainText": "Hence, any academic decision should be approached with utmost caution.",
                    "OptionText": null,
                    "NextItemIDList": [
                        201200029
                    ],
                    "Sender": "NPC"
                },
                "201200029": {
                    "Id": 201200029,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": null,
                    "Name": null,
                    "IconPath": null,
                    "MainText": "Though it may sound harsh, not everyone is suited for the scholarly pursuit.",
                    "OptionText": null,
                    "NextItemIDList": [
                        201200030,
                        201200031
                    ],
                    "Sender": "NPC"
                },
                "201200030": {
                    "Id": 201200030,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": null,
                    "Name": null,
                    "IconPath": null,
                    "MainText": "I'll give it more serious thoughts",
                    "OptionText": "I'll give it more serious thoughts",
                    "NextItemIDList": [
                        201200032
                    ],
                    "Sender": "Player"
                },
                "201200031": {
                    "Id": 201200031,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": null,
                    "Name": null,
                    "IconPath": null,
                    "MainText": "I'm surprised you're aware it sounds harsh",
                    "OptionText": "I'm surprised you're aware it sounds harsh",
                    "NextItemIDList": [
                        201200032
                    ],
                    "Sender": "Player"
                },
                "201200032": {
                    "Id": 201200032,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": null,
                    "Name": null,
                    "IconPath": null,
                    "MainText": "Do ponder upon it with due diligence before tendering your response, and should you require it, I shall gladly lend my quill to pen a commendatory missive to your benefit.",
                    "OptionText": null,
                    "NextItemIDList": [],
                    "Sender": "NPC"
                }
            },
            "MessageStart": 201200000
        }
    ]
}