            "Messages": {
                "131140001": {
                    "Id": 131140001,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": 1305,
                    "Name": "Dr. Ratio",
                    "IconPath": "SpriteOutput/AvatarRoundIcon/Avatar/1305.png",
                    "MainText": "How is testing the Divergent Universe? The Human Comedy, heh... Screwllum surprised me. His kingly qualities indeed mean he's more open to accepting external suggestions, unlike the other Geniuses.",
                    "OptionText": null,
                    "NextItemIDList": [
                        131140002
                    ],
                    "Sender": "NPC"
                },
                "131140002": {
                    "Id": 131140002,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": 1305,
                    "Name": "Dr. Ratio",
                    "IconPath": "SpriteOutput/AvatarRoundIcon/Avatar/1305.png",
                    "MainText": "In my spare time, I've compiled a new batch of data as an iterative sample for this version of the Divergent Universe.",
                    "OptionText": null,
                    "NextItemIDList": [
                        131140003
                    ],
                    "Sender": "NPC"
                },
                "131140003": {
                    "Id": 131140003,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": 1305,
                    "Name": "Dr. Ratio",
                    "IconPath": "SpriteOutput/AvatarRoundIcon/Avatar/1305.png",
                    "MainText": "I'll send you a copy. Think of it as practice material for a test.",
                    "OptionText": null,
                    "NextItemIDList": [
                        131140004
                    ],
                    "Sender": "NPC"
                },
                "131140004": {
                    "Id": 131140004,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": null,
                    "Name": null,
                    "IconPath": null,
                    "MainText": "Attachment <i>Investigation on the Lifecycle of Cosmic Intelligent Life Forms - AE <unbreak>2157</unbreak></i> has been sent",
                    "OptionText": null,
                    "NextItemIDList": [
                        131140005,
                        131140006,
                        131140009
                    ],
                    "Sender": "System"
                },
                "131140005": {
                    "Id": 131140005,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": null,
                    "Name": null,
                    "IconPath": null,
                    "MainText": "I'm only a tester. Thinking is not in my job description.",
                    "OptionText": "I'm only a tester",
                    "NextItemIDList": [
                        131140011
                    ],
                    "Sender": "Player"
                },
                "131140006": {
                    "Id": 131140006,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": null,
                    "Name": null,
                    "IconPath": null,
                    "MainText": "On it. I love learning",
                    "OptionText": "On it",
                    "NextItemIDList": [
                        131140007
                    ],
                    "Sender": "Player"
                },
                "131140009": {
                    "Id": 131140009,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": null,
                    "Name": null,
                    "IconPath": null,
                    "MainText": "Um... Have we met?",
                    "OptionText": "Who are you?",
                    "NextItemIDList": [
                        131140010
                    ],
                    "Sender": "Player"
                },
                "131140011": {
                    "Id": 131140011,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": 1305,
                    "Name": "Dr. Ratio",
                    "IconPath": "SpriteOutput/AvatarRoundIcon/Avatar/1305.png",
                    "MainText": "The Equations allow you to experience different lives. It's not about making you become someone, but for you to have a clear idea of where you want to go.",
                    "OptionText": null,
                    "NextItemIDList": [
                        131140012
                    ],
                    "Sender": "NPC"
                },
                "131140007": {
                    "Id": 131140007,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": null,
                    "Name": null,
                    "IconPath": null,
                    "MainText": "By the way, how long would it take to finish reading the document?",
                    "OptionText": null,
                    "NextItemIDList": [
                        131140008
                    ],
                    "Sender": "PlayerAuto"
                },
                "131140010": {
                    "Id": 131140010,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": 1305,
                    "Name": "Dr. Ratio",
                    "IconPath": "SpriteOutput/AvatarRoundIcon/Avatar/1305.png",
                    "MainText": "It doesn't matter if you know me or not, but you should know yourself and the data I sent. That's very important.",
                    "OptionText": null,
                    "NextItemIDList": [
                        131140011
                    ],
                    "Sender": "NPC"
                },
                "131140012": {
                    "Id": 131140012,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": 1305,
                    "Name": "Dr. Ratio",
                    "IconPath": "SpriteOutput/AvatarRoundIcon/Avatar/1305.png",
                    "MainText": "Even if the genius' research doesn't concern you, you shouldn't stop pondering the subject of \"life.\"",
                    "OptionText": null,
                    "NextItemIDList": [
                        131140013
                    ],
                    "Sender": "NPC"
                },
                "131140008": {
                    "Id": 131140008,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": 1305,
                    "Name": "Dr. Ratio",
                    "IconPath": "SpriteOutput/AvatarRoundIcon/Avatar/1305.png",
                    "MainText": "Conservatively? Just 1 Amber Era.",
                    "OptionText": null,
                    "NextItemIDList": [
                        131140011
                    ],
                    "Sender": "NPC"
                },
                "131140013": {
                    "Id": 131140013,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": 1305,
                    "Name": "Dr. Ratio",
                    "IconPath": "SpriteOutput/AvatarRoundIcon/Avatar/1305.png",
                    "MainText": "Don't worry. For users such as yourself, just reading the literature review and the conclusion sections will suffice.",
                    "OptionText": null,
                    "NextItemIDList": [
                        131140014
                    ],
                    "Sender": "NPC"
                },
                "131140014": {
                    "Id": 131140014,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": 1305,
                    "Name": "Dr. Ratio",
                    "IconPath": "SpriteOutput/AvatarRoundIcon/Avatar/1305.png",
                    "MainText": "Not even Screwllum will read through this verbose document word by word.",
                    "OptionText": null,
                    "NextItemIDList": [
                        131140015
                    ],
                    "Sender": "NPC"
                },
                "131140015": {
                    "Id": 131140015,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": null,
                    "Name": null,
                    "IconPath": null,
                    "MainText": "You are the partner Screwllum mentioned?",
                    "OptionText": "Are you working together?",
                    "NextItemIDList": [
                        131140016
                    ],
                    "Sender": "Player"
                },
                "131140016": {
                    "Id": 131140016,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": 1305,
                    "Name": "Dr. Ratio",
                    "IconPath": "SpriteOutput/AvatarRoundIcon/Avatar/1305.png",
                    "MainText": "Screwllum is trying to understand and resolve the differences and conflicts among intelligent lifeforms. I only provided him with some materials and advice during academic exchanges.",
                    "OptionText": null,
                    "NextItemIDList": [
                        131140017
                    ],
                    "Sender": "NPC"
                },
                "131140017": {
                    "Id": 131140017,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": 1305,
                    "Name": "Dr. Ratio",
                    "IconPath": "SpriteOutput/AvatarRoundIcon/Avatar/1305.png",
                    "MainText": "As for \"partner\"... that's just him overstating my contribution.",
                    "OptionText": null,
                    "NextItemIDList": [
                        131140018,
                        131140020
                    ],
                    "Sender": "NPC"
                },
                "131140018": {
                    "Id": 131140018,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": null,
                    "Name": null,
                    "IconPath": null,
                    "MainText": "I have met many people. If Screwllum says that they're his partner...",
                    "OptionText": "As expected of you",
                    "NextItemIDList": [
                        131140019
                    ],
                    "Sender": "Player"
                },
                "131140020": {
                    "Id": 131140020,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": null,
                    "Name": null,
                    "IconPath": null,
                    "MainText": "He thinks highly of you",
                    "OptionText": "He thinks highly of you",
                    "NextItemIDList": [
                        131140021
                    ],
                    "Sender": "Player"
                },
                "131140019": {
                    "Id": 131140019,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": null,
                    "Name": null,
                    "IconPath": null,
                    "MainText": "They'd be so happy that they might even burst out laughing in the middle of the night.",
                    "OptionText": null,
                    "NextItemIDList": [
                        131140022
                    ],
                    "Sender": "PlayerAuto"
                },
                "131140021": {
                    "Id": 131140021,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": null,
                    "Name": null,
                    "IconPath": null,
                    "MainText": "Yet you declined his invitation, why?",
                    "OptionText": null,
                    "NextItemIDList": [
                        131140022
                    ],
                    "Sender": "PlayerAuto"
                },
                "131140022": {
                    "Id": 131140022,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": 1305,
                    "Name": "Dr. Ratio",
                    "IconPath": "SpriteOutput/AvatarRoundIcon/Avatar/1305.png",
                    "MainText": "Geniuses are like stars. The value they create can illuminate an entire planetary system.",
                    "OptionText": null,
                    "NextItemIDList": [
                        131140023
                    ],
                    "Sender": "NPC"
                },
                "131140023": {
                    "Id": 131140023,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": 1305,
                    "Name": "Dr. Ratio",
                    "IconPath": "SpriteOutput/AvatarRoundIcon/Avatar/1305.png",
                    "MainText": "The mundane is always trying to follow their footsteps, only to be blinded and become lost.",
                    "OptionText": null,
                    "NextItemIDList": [
                        131140024
                    ],
                    "Sender": "NPC"
                },
                "131140024": {
                    "Id": 131140024,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": 1305,
                    "Name": "Dr. Ratio",
                    "IconPath": "SpriteOutput/AvatarRoundIcon/Avatar/1305.png",
                    "MainText": "He has his topics, and I have mine.",
                    "OptionText": null,
                    "NextItemIDList": [
                        131140025
                    ],
                    "Sender": "NPC"
                },
                "131140025": {
                    "Id": 131140025,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": 1305,
                    "Name": "Dr. Ratio",
                    "IconPath": "SpriteOutput/AvatarRoundIcon/Avatar/1305.png",
                    "MainText": "One needs to walk their own path. This is true to both you and me.",
                    "OptionText": null,
                    "NextItemIDList": [],
                    "Sender": "NPC"
                }
            },
            "MessageStart": 131140001
        }
    ]
