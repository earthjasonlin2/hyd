    "Sections": [
        {
            "Id": 2012200,
            "Messages": {
                "201220000": {
                    "Id": 201220000,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": null,
                    "Name": null,
                    "IconPath": null,
                    "MainText": "Having a chance to see the return of the Astral Express truly is a joyous thing.",
                    "OptionText": null,
                    "NextItemIDList": [
                        201220001
                    ],
                    "Sender": "NPC"
                },
                "201220001": {
                    "Id": 201220001,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": null,
                    "Name": null,
                    "IconPath": null,
                    "MainText": "When going to other worlds, I always came across memories related to the Express. As time has gone on, I feel like we have become friends that have never met.",
                    "OptionText": null,
                    "NextItemIDList": [
                        201220002
                    ],
                    "Sender": "NPC"
                },
                "201220002": {
                    "Id": 201220002,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": null,
                    "Name": null,
                    "IconPath": null,
                    "MainText": "For example?",
                    "OptionText": "For example...",
                    "NextItemIDList": [
                        201220003
                    ],
                    "Sender": "Player"
                },
                "201220003": {
                    "Id": 201220003,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": null,
                    "Name": null,
                    "IconPath": null,
                    "MainText": "I once visited a world that stood high up in the sky. The people of this world hid amongst the clouds, driving huge monsters to bring down the rainwater.",
                    "OptionText": null,
                    "NextItemIDList": [
                        201220004
                    ],
                    "Sender": "NPC"
                },
                "201220004": {
                    "Id": 201220004,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": null,
                    "Name": null,
                    "IconPath": null,
                    "MainText": "In their legends, these huge monsters were brought there by a giant Nameless, allowing their local civilization to thrive.",
                    "OptionText": null,
                    "NextItemIDList": [
                        201220005
                    ],
                    "Sender": "NPC"
                },
                "201220005": {
                    "Id": 201220005,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": null,
                    "Name": null,
                    "IconPath": null,
                    "MainText": "I also once visited a world that rained gems. The people of this world predict the weather, collect the gems and use them in trade with other worlds. Their legends also state that this was also a revelation brought about by a Nameless that traveled on the Express.",
                    "OptionText": null,
                    "NextItemIDList": [
                        201220006
                    ],
                    "Sender": "NPC"
                },
                "201220006": {
                    "Id": 201220006,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": null,
                    "Name": null,
                    "IconPath": null,
                    "MainText": "Hearing all of these legends that sounded so unbelievable, I could not help but wonder what the life of a Nameless is truly like.",
                    "OptionText": null,
                    "NextItemIDList": [
                        201220007,
                        201220009
                    ],
                    "Sender": "NPC"
                },
                "201220007": {
                    "Id": 201220007,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": null,
                    "Name": null,
                    "IconPath": null,
                    "MainText": "Though I have not been to many worlds yet, the Nameless are indeed the kind of people that will assist those around them wherever they go",
                    "OptionText": "Very similar!",
                    "NextItemIDList": [
                        201220008
                    ],
                    "Sender": "Player"
                },
                "201220009": {
                    "Id": 201220009,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": null,
                    "Name": null,
                    "IconPath": null,
                    "MainText": "These legends are so magnificent, but our daily lives tend to be quite unremarkable most of the time",
                    "OptionText": "Very different!",
                    "NextItemIDList": [
                        201220010
                    ],
                    "Sender": "Player"
                },
                "201220008": {
                    "Id": 201220008,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": null,
                    "Name": null,
                    "IconPath": null,
                    "MainText": "But it's not as miraculous and astounding as these legends",
                    "OptionText": null,
                    "NextItemIDList": [
                        201220011
                    ],
                    "Sender": "PlayerAuto"
                },
                "201220010": {
                    "Id": 201220010,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": null,
                    "Name": null,
                    "IconPath": null,
                    "MainText": "However, the Nameless are indeed the kind of people that will assist those around them wherever they go",
                    "OptionText": null,
                    "NextItemIDList": [
                        201220011
                    ],
                    "Sender": "PlayerAuto"
                },
                "201220011": {
                    "Id": 201220011,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": null,
                    "Name": null,
                    "IconPath": null,
                    "MainText": "It seems to be a path full of both challenges and rewards. I wonder what has allowed the Express and the Nameless to persist for all this time?",
                    "OptionText": null,
                    "NextItemIDList": [
                        201220012,
                        201220013,
                        201220014
                    ],
                    "Sender": "NPC"
                },
                "201220012": {
                    "Id": 201220012,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": null,
                    "Name": null,
                    "IconPath": null,
                    "MainText": "We all practice the values of the Trailblaze, which is the Nameless' motivation to keep moving forwards",
                    "OptionText": "Conviction",
                    "NextItemIDList": [
                        201220015
                    ],
                    "Sender": "Player"
                },
                "201220013": {
                    "Id": 201220013,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": null,
                    "Name": null,
                    "IconPath": null,
                    "MainText": "It is likely the legacy passed down from generation to generation of Nameless. Though one Trailblazer is lost, the embers they leave behind are passed down to ignite the passion of the next generation.",
                    "OptionText": "Legacy",
                    "NextItemIDList": [
                        201220015
                    ],
                    "Sender": "Player"
                },
                "201220014": {
                    "Id": 201220014,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": null,
                    "Name": null,
                    "IconPath": null,
                    "MainText": "I imagine it is the amazing opportunities offered up by the Express. As long as one is willing to set forth together on an adventure, the Express's doors will freely open to them.",
                    "OptionText": "Openness",
                    "NextItemIDList": [
                        201220015
                    ],
                    "Sender": "Player"
                },
                "201220015": {
                    "Id": 201220015,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": null,
                    "Name": null,
                    "IconPath": null,
                    "MainText": "Our path is unlike that which you have chosen with the Express, as you advance to distant lands, we spend our time seeking the past. You form a bond and travel together, where we often journey alone.",
                    "OptionText": null,
                    "NextItemIDList": [
                        201220016
                    ],
                    "Sender": "NPC"
                },
                "201220016": {
                    "Id": 201220016,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": null,
                    "Name": null,
                    "IconPath": null,
                    "MainText": "However, in this vast existence, the footprints of both Memokeepers and Nameless often overlap. Perhaps we shall meet each other at our final destination.",
                    "OptionText": null,
                    "NextItemIDList": [
                        201220017
                    ],
                    "Sender": "NPC"
                },
                "201220017": {
                    "Id": 201220017,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": null,
                    "Name": null,
                    "IconPath": null,
                    "MainText": "What is the life of a Memokeeper like?",
                    "OptionText": "The life of a Memokeeper...",
                    "NextItemIDList": [
                        201220018
                    ],
                    "Sender": "Player"
                },
                "201220018": {
                    "Id": 201220018,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": null,
                    "Name": null,
                    "IconPath": null,
                    "MainText": "Memokeepers live a memetic existence, traveling through worlds through the medium of concepts and memories.",
                    "OptionText": null,
                    "NextItemIDList": [
                        201220019
                    ],
                    "Sender": "NPC"
                },
                "201220019": {
                    "Id": 201220019,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": null,
                    "Name": null,
                    "IconPath": null,
                    "MainText": "Like a passing visitor in the lives of others, I do not often get involved in the worlds I visit as all we care for are the memories contained within.",
                    "OptionText": null,
                    "NextItemIDList": [
                        201220020,
                        201220022
                    ],
                    "Sender": "NPC"
                },
                "201220020": {
                    "Id": 201220020,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": null,
                    "Name": null,
                    "IconPath": null,
                    "MainText": "Travelling through concepts, that sounds so cool!",
                    "OptionText": "That sounds cool",
                    "NextItemIDList": [
                        201220021
                    ],
                    "Sender": "Player"
                },
                "201220022": {
                    "Id": 201220022,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": null,
                    "Name": null,
                    "IconPath": null,
                    "MainText": "That sounds quite boring...",
                    "OptionText": "That sounds boring",
                    "NextItemIDList": [
                        201220023
                    ],
                    "Sender": "Player"
                },
                "201220021": {
                    "Id": 201220021,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": null,
                    "Name": null,
                    "IconPath": null,
                    "MainText": "Indeed, entering another's memory is like walking into a whole new world.",
                    "OptionText": null,
                    "NextItemIDList": [
                        201220024
                    ],
                    "Sender": "NPC"
                },
                "201220023": {
                    "Id": 201220023,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": null,
                    "Name": null,
                    "IconPath": null,
                    "MainText": "Memories are the most precious when they have not been interfered with by others.",
                    "OptionText": null,
                    "NextItemIDList": [
                        201220024
                    ],
                    "Sender": "NPC"
                },
                "201220024": {
                    "Id": 201220024,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": null,
                    "Name": null,
                    "IconPath": null,
                    "MainText": "In the future, if you come across any interesting stories, don't forget to come and share them with me.",
                    "OptionText": null,
                    "NextItemIDList": [
                        201220025
                    ],
                    "Sender": "NPC"
                },
                "201220025": {
                    "Id": 201220025,
