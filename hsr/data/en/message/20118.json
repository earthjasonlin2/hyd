                    "NextItemIDList": [
                        201180034
                    ],
                    "Sender": "NPC"
                },
                "201180036": {
                    "Id": 201180036,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": null,
                    "Name": null,
                    "IconPath": null,
                    "MainText": "You'll have lost the game, if that is your argument.",
