                    "OptionText": null,
                    "NextItemIDList": [
                        201180039
                    ],
                    "Sender": "NPC"
                },
                "201180039": {
                    "Id": 201180039,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": null,
                    "Name": null,
                    "IconPath": null,
