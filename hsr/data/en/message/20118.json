                "201180009": {
                    "Id": 201180009,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": null,
                    "Name": null,
                    "IconPath": null,
                    "MainText": "All the more reason for you to learn, then.",
                    "OptionText": null,
                    "NextItemIDList": [
                        201180012
                    ],
                    "Sender": "NPC"
                },
                "201180011": {
                    "Id": 201180011,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": null,
                    "Name": null,
                    "IconPath": null,
                    "MainText": "Clearly, you don't know yourself well enough.",
                    "OptionText": null,
                    "NextItemIDList": [
                        201180012
                    ],
                    "Sender": "NPC"
                },
                "201180007": {
                    "Id": 201180007,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": null,
                    "Name": null,
                    "IconPath": null,
                    "MainText": "There are no bounds in the art of language.",
                    "OptionText": null,
                    "NextItemIDList": [
                        201180012
                    ],
                    "Sender": "NPC"
                },
                "201180012": {
                    "Id": 201180012,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": null,
                    "Name": null,
                    "IconPath": null,
                    "MainText": "It matters not whether you join the competition.",
                    "OptionText": null,
                    "NextItemIDList": [
                        201180013
                    ],
                    "Sender": "NPC"
                },
                "201180013": {
                    "Id": 201180013,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": null,
                    "Name": null,
                    "IconPath": null,
                    "MainText": "But I am inclined to acquire an understanding of how you perceive debate as an activity within your cognitive framework.",
                    "OptionText": null,
                    "NextItemIDList": [
                        201180014,
                        201180015,
                        201180016
                    ],
                    "Sender": "NPC"
                },
                "201180014": {
                    "Id": 201180014,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": null,
                    "Name": null,
                    "IconPath": null,
                    "MainText": "Debate is about arguing against the other side",
                    "OptionText": "Arguing against the other side",
                    "NextItemIDList": [
                        201180017
                    ],
                    "Sender": "Player"
                },
                "201180015": {
                    "Id": 201180015,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": null,
                    "Name": null,
                    "IconPath": null,
                    "MainText": "Debate is about triggering the other side",
                    "OptionText": "Angering your opponent",
                    "NextItemIDList": [
                        201180017
                    ],
                    "Sender": "Player"
                },
                "201180016": {
                    "Id": 201180016,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": null,
                    "Name": null,
                    "IconPath": null,
