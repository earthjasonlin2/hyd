                "111240001": {
                    "Id": 111240001,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": null,
                    "Name": null,
                    "IconPath": null,
                    "MainText": "I have a question for you.\\nDid March, Dan Heng, and you see anything extraordinary in the mountains north of Belobog?",
                    "OptionText": null,
                    "NextItemIDList": [
                        111240002,
                        111240006,
                        111240010
                    ],
                    "Sender": "NPC"
                },
                "111240002": {
                    "Id": 111240002,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": null,
                    "Name": null,
                    "IconPath": null,
                    "MainText": "There was a giant robot called the Engine of Creation",
                    "OptionText": "We saw the Engine of Creation",
                    "NextItemIDList": [
                        111240003
                    ],
                    "Sender": "Player"
                },
                "111240006": {
                    "Id": 111240006,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": null,
                    "Name": null,
                    "IconPath": null,
                    "MainText": "I recall that there was a device to imprison the Stellaron",
                    "OptionText": "We saw a device to imprison the Stellaron",
                    "NextItemIDList": [
                        111240007
                    ],
                    "Sender": "Player"
                },
                "111240010": {
                    "Id": 111240010,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": null,
                    "Name": null,
                    "IconPath": null,
                    "MainText": "I think I saw lots of March 8ths",
                    "OptionText": "We saw March 8th",
                    "NextItemIDList": [
                        111240011
                    ],
                    "Sender": "Player"
                },
                "111240003": {
                    "Id": 111240003,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": null,
                    "Name": null,
                    "IconPath": null,
                    "MainText": "That might be what we are looking for. It would be good if I can witness it with my own eyes.\\nMaybe it's possible to get it re-activated",
                    "OptionText": null,
                    "NextItemIDList": [
                        111240004
                    ],
                    "Sender": "NPC"
                },
                "111240007": {
                    "Id": 111240007,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": null,
                    "Name": null,
                    "IconPath": null,
                    "MainText": "The Stellaron is indeed capable of releasing unusual electromagnetic waves. Maybe that is what we are looking for",
                    "OptionText": null,
                    "NextItemIDList": [
                        111240013
                    ],
                    "Sender": "NPC"
                },
