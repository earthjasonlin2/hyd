            "Messages": {
                "121040109": {
                    "Id": 121040109,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": null,
                    "Name": null,
                    "IconPath": null,
                    "MainText": "Luhui, I want to ask you something",
                    "OptionText": null,
                    "NextItemIDList": [
                        121040100,
                        121040101
                    ],
                    "Sender": "PlayerAuto"
                },
                "121040100": {
                    "Id": 121040100,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": null,
                    "Name": null,
                    "IconPath": null,
                    "MainText": "Are you at the Alchemy Commission yet? The Sky-Faring Commission has some questions for Mei",
                    "OptionText": "Are you at the Alchemy Commission already?",
                    "NextItemIDList": [
                        121040102
                    ],
                    "Sender": "Player"
                },
                "121040101": {
                    "Id": 121040101,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": null,
                    "Name": null,
                    "IconPath": null,
                    "MainText": "Have you seen Mei yet? The Sky-Faring Commission has some questions for her",
                    "OptionText": "Have you seen Mei yet?",
                    "NextItemIDList": [
                        121040102
                    ],
                    "Sender": "Player"
                },
                "121040102": {
                    "Id": 121040102,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": 32,
                    "Name": "Luhui",
                    "IconPath": "SpriteOutput/AvatarRoundIcon/UI_Message_Contacts_Luhui.png",
                    "MainText": "I just arrived and heard some people at the Alchemy Commission say that Mei left when no one was looking",
                    "OptionText": null,
                    "NextItemIDList": [
                        121040103
                    ],
                    "Sender": "NPC"
                },
                "121040103": {
                    "Id": 121040103,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": 32,
                    "Name": "Luhui",
                    "IconPath": "SpriteOutput/AvatarRoundIcon/UI_Message_Contacts_Luhui.png",
                    "MainText": "Oh, this girl! So obstinate and stubborn!",
                    "OptionText": null,
                    "NextItemIDList": [
                        121040104
                    ],
                    "Sender": "NPC"
                },
                "121040104": {
                    "Id": 121040104,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": 32,
                    "Name": "Luhui",
                    "IconPath": "SpriteOutput/AvatarRoundIcon/UI_Message_Contacts_Luhui.png",
                    "MainText": "I asked around and found out that there are some missing pages for some \"notes\"? Mei seems determined to find them",
                    "OptionText": null,
                    "NextItemIDList": [
                        121040105
                    ],
                    "Sender": "NPC"
                },
                "121040105": {
                    "Id": 121040105,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": 32,
                    "Name": "Luhui",
                    "IconPath": "SpriteOutput/AvatarRoundIcon/UI_Message_Contacts_Luhui.png",
                    "MainText": "You have a few pages with you, right?",
                    "OptionText": null,
                    "NextItemIDList": [
                        121040106
                    ],
                    "Sender": "NPC"
                },
                "121040106": {
                    "Id": 121040106,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": null,
                    "Name": null,
                    "IconPath": null,
                    "MainText": "I found her notes at the accident site and found your contact details",
                    "OptionText": "I picked up the notes",
                    "NextItemIDList": [
                        121040107
                    ],
                    "Sender": "Player"
                },
                "121040107": {
                    "Id": 121040107,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": 32,
                    "Name": "Luhui",
                    "IconPath": "SpriteOutput/AvatarRoundIcon/UI_Message_Contacts_Luhui.png",
                    "MainText": "I had taken leave for this, but things on set are pretty busy. I have to go back now. I just can't run around following her everywhere. I already paid for her medical bill, so please tell her to go back to the apothecary as soon as possible",
                    "OptionText": null,
                    "NextItemIDList": [
                        121040108
                    ],
                    "Sender": "NPC"
                },
                "121040108": {
                    "Id": 121040108,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": 32,
                    "Name": "Luhui",
                    "IconPath": "SpriteOutput/AvatarRoundIcon/UI_Message_Contacts_Luhui.png",
                    "MainText": "Please!",
                    "OptionText": null,
                    "NextItemIDList": [],
                    "Sender": "NPC"
                }
            },
            "MessageStart": 121040109
        },
        {
            "Id": 1210301,
            "Messages": {
                "121030100": {
                    "Id": 121030100,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": null,
                    "Name": null,
                    "IconPath": null,
                    "MainText": "The Sky-Faring Commission has already apprehended the culprit. No need to worry about the subsequent penalties and compensation",
                    "OptionText": null,
                    "NextItemIDList": [
                        121030101
                    ],
                    "Sender": "PlayerAuto"
                },
                "121030101": {
                    "Id": 121030101,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": 32,
                    "Name": "Luhui",
                    "IconPath": "SpriteOutput/AvatarRoundIcon/UI_Message_Contacts_Luhui.png",
                    "MainText": "Sorry for troubling you with all this. I'll help Mei with the subsequent penalties. But I'm a little confused. Do I need to continue talking with Mei about her dismissal...?",
                    "OptionText": null,
                    "NextItemIDList": [
                        121030102
                    ],
                    "Sender": "NPC"
                },
                "121030102": {
                    "Id": 121030102,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": 32,
                    "Name": "Luhui",
                    "IconPath": "SpriteOutput/AvatarRoundIcon/UI_Message_Contacts_Luhui.png",
                    "MainText": "In employment, is the exchange of services for payment the most important thing? Or is it the interpersonal relationship it created?",
                    "OptionText": null,
                    "NextItemIDList": [
                        121030103,
                        121030104,
                        121030106
                    ],
                    "Sender": "NPC"
                },
                "121030103": {
                    "Id": 121030103,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": null,
                    "Name": null,
                    "IconPath": null,
                    "MainText": "You two are in an employment relationship. Service is exchanged for payment. While it's good to have a healthy relationship between you, this should be distinct from the professional relationship",
                    "OptionText": "Being paid for labor is more important",
                    "NextItemIDList": [
                        121030108
                    ],
                    "Sender": "Player"
                },
                "121030104": {
                    "Id": 121030104,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": null,
                    "Name": null,
                    "IconPath": null,
                    "MainText": "I do believe that money is perhaps the most reliable value indicator in all employment. But in the end, all the money in the world is only material possession",
                    "OptionText": "Interpersonal ties are more important",
                    "NextItemIDList": [
                        121030105
                    ],
                    "Sender": "Player"
                },
                "121030106": {
                    "Id": 121030106,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": null,
                    "Name": null,
                    "IconPath": null,
                    "MainText": "I'm just an outsider. My opinion isn't important to the relationship between you two",
                    "OptionText": "It seems you already have your answer",
                    "NextItemIDList": [
                        121030107
                    ],
                    "Sender": "Player"
                },
                "121030108": {
                    "Id": 121030108,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": 32,
                    "Name": "Luhui",
                    "IconPath": "SpriteOutput/AvatarRoundIcon/UI_Message_Contacts_Luhui.png",
                    "MainText": "In that case, both Mei and I are very grateful for you. Let's meet in Starskiff Haven. I still have something for you to show my appreciation",
                    "OptionText": null,
                    "NextItemIDList": [
                        121030109
                    ],
                    "Sender": "NPC"
                },
                "121030105": {
                    "Id": 121030105,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": null,
                    "Name": null,
                    "IconPath": null,
                    "MainText": "You'll miss out on a lot of things that you can't put a price tag on, like the emotional bond between you two",
                    "OptionText": null,
                    "NextItemIDList": [
                        121030108
                    ],
                    "Sender": "PlayerAuto"
                },
                "121030107": {
                    "Id": 121030107,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": null,
                    "Name": null,
                    "IconPath": null,
                    "MainText": "You might've come to me for my opinion, but I think you already know the answer",
                    "OptionText": null,
                    "NextItemIDList": [
                        121030108
                    ],
                    "Sender": "PlayerAuto"
                },
                "121030109": {
                    "Id": 121030109,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": 32,
                    "Name": "Luhui",
                    "IconPath": "SpriteOutput/AvatarRoundIcon/UI_Message_Contacts_Luhui.png",
                    "MainText": "...I don't know why, but with you, I'm able to say things I could never say before",
                    "OptionText": null,
                    "NextItemIDList": [],
                    "Sender": "NPC"
                }
            },
            "MessageStart": 121030100
        }
