                "201310024": {
                    "Id": 201310024,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": null,
                    "Name": null,
                    "IconPath": null,
                    "MainText": "Can't quite recall the exact date. I was out doin patrol round the streets when I stumbled upon it in this dodgy ol' alley. It was brawling with a few other dogs.",
                    "OptionText": null,
                    "NextItemIDList": [
                        201310025
