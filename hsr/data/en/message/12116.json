                    "ItemContentID": null,
                    "ContactsID": 1201,
                    "Name": "Qingque",
                    "IconPath": "SpriteOutput/AvatarRoundIcon/Avatar/1201.png",
                    "MainText": "Then that's easy",
                    "OptionText": null,
                    "NextItemIDList": [
                        121160017
                    ],
                    "Sender": "NPC"
                },
                "121160009": {
                    "Id": 121160009,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": 1201,
                    "Name": "Qingque",
                    "IconPath": "SpriteOutput/AvatarRoundIcon/Avatar/1201.png",
