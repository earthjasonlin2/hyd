                "121160000": {
                    "Id": 121160000,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": 1201,
                    "Name": "Qingque",
                    "IconPath": "SpriteOutput/AvatarRoundIcon/Avatar/1201.png",
                    "MainText": "Hey there. I heard folks from the Artisanship Commission say that you helped clear the Ambrosial Arbor coils. You're indeed as saintly as the general says you are",
                    "OptionText": null,
                    "NextItemIDList": [
                        121160001,
                        121160002,
                        121160003
                    ],
                    "Sender": "NPC"
                },
                "121160001": {
                    "Id": 121160001,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": null,
                    "Name": null,
                    "IconPath": null,
                    "MainText": "Saintly? That doesn't sound much like a compliment",
                    "OptionText": "It doesn't sound like a compliment to me.",
                    "NextItemIDList": [
                        121160004
                    ],
                    "Sender": "Player"
                },
                "121160002": {
                    "Id": 121160002,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": null,
                    "Name": null,
                    "IconPath": null,
                    "MainText": "Don't beat around the bush. What's the matter?",
                    "OptionText": "Sounds like you need a favor",
                    "NextItemIDList": [
                        121160010
                    ],
                    "Sender": "Player"
                },
                "121160003": {
                    "Id": 121160003,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": null,
                    "Name": null,
                    "IconPath": null,
                    "MainText": "I am brave and kind, and I love to help others.",
                    "OptionText": "That's me alright!",
                    "NextItemIDList": [
                        121160012
                    ],
                    "Sender": "Player"
                },
                "121160004": {
                    "Id": 121160004,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": 1201,
                    "Name": "Qingque",
                    "IconPath": "SpriteOutput/AvatarRoundIcon/Avatar/1201.png",
                    "MainText": "Of course it's a compliment. I thank you from the bottom of my heart. Only when busy people get busy do slackers get a chance to slack off",
                    "OptionText": null,
                    "NextItemIDList": [
                        121160005
                    ],
                    "Sender": "NPC"
                },
                "121160010": {
                    "Id": 121160010,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": 1201,
                    "Name": "Qingque",
                    "IconPath": "SpriteOutput/AvatarRoundIcon/Avatar/1201.png",
                    "MainText": "It seems that you are already used to the pleasantries people use on Xianzhou, and know when the work will come.\\nYou will go far, young {F#lady}{M#man}! I believe you will soon understand my three basic principles for happiness at work",
                    "OptionText": null,
                    "NextItemIDList": [
                        121160011
                    ],
                    "Sender": "NPC"
                },
                "121160012": {
                    "Id": 121160012,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": 1201,
                    "Name": "Qingque",
                    "IconPath": "SpriteOutput/AvatarRoundIcon/Avatar/1201.png",
                    "MainText": "Great! A brave and kind person like you can definitely help me",
                    "OptionText": null,
                    "NextItemIDList": [
                        121160013
                    ],
                    "Sender": "NPC"
                },
                "121160005": {
                    "Id": 121160005,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": 1201,
                    "Name": "Qingque",
                    "IconPath": "SpriteOutput/AvatarRoundIcon/Avatar/1201.png",
                    "MainText": "But, based on your past experience, won't you get a sort of a hunch when I message flatteries to you?",
                    "OptionText": null,
                    "NextItemIDList": [
                        121160006
                    ],
                    "Sender": "NPC"
                },
                "121160011": {
                    "Id": 121160011,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": 1201,
                    "Name": "Qingque",
                    "IconPath": "SpriteOutput/AvatarRoundIcon/Avatar/1201.png",
                    "MainText": "But the trouble comes from me this time. Please don't shrug it off and do help me",
                    "OptionText": null,
                    "NextItemIDList": [
                        121160013
                    ],
                    "Sender": "NPC"
                },
                "121160013": {
                    "Id": 121160013,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": 1201,
                    "Name": "Qingque",
                    "IconPath": "SpriteOutput/AvatarRoundIcon/Avatar/1201.png",
                    "MainText": "Do you remember the craftsman apprentice named Chengjie from the Artisanship Commission?",
                    "OptionText": null,
                    "NextItemIDList": [
                        121160014,
                        121160015
                    ],
                    "Sender": "NPC"
                },
                "121160006": {
                    "Id": 121160006,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": 1201,
                    "Name": "Qingque",
                    "IconPath": "SpriteOutput/AvatarRoundIcon/Avatar/1201.png",
                    "MainText": "...A hunch? Like, this person wants something from me?",
                    "OptionText": null,
                    "NextItemIDList": [
                        121160007,
                        121160008
                    ],
                    "Sender": "NPC"
                },
                "121160014": {
                    "Id": 121160014,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": null,
                    "Name": null,
                    "IconPath": null,
                    "MainText": "I don't remember",
                    "OptionText": "I don't remember",
                    "NextItemIDList": [
                        121160016
                    ],
                    "Sender": "Player"
                },
                "121160015": {
                    "Id": 121160015,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": null,
                    "Name": null,
                    "IconPath": null,
                    "MainText": "Chengjie, the apprentice... I think I remember him",
                    "OptionText": "I kind of remember",
                    "NextItemIDList": [
                        121160020
                    ],
                    "Sender": "Player"
                },
                "121160007": {
                    "Id": 121160007,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": null,
                    "Name": null,
                    "IconPath": null,
                    "MainText": "So, what do you want from me?",
                    "OptionText": "What do you want from me?",
                    "NextItemIDList": [
                        121160013
                    ],
                    "Sender": "Player"
                },
                "121160008": {
                    "Id": 121160008,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": null,
                    "Name": null,
                    "IconPath": null,
                    "MainText": "How about this? You recall the message now, and we forget about the whole thing",
                    "OptionText": "Can I pretend not to know?",
                    "NextItemIDList": [
                        121160009
                    ],
                    "Sender": "Player"
                },
                "121160016": {
                    "Id": 121160016,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": 1201,
                    "Name": "Qingque",
                    "IconPath": "SpriteOutput/AvatarRoundIcon/Avatar/1201.png",
                    "MainText": "It's okay if you don't remember. Come to the Artisanship Commission. I'll show him to you",
                    "OptionText": null,
