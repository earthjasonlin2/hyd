                "200640000": {
                    "Id": 200640000,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": null,
                    "Name": null,
                    "IconPath": null,
                    "MainText": "{NICKNAME}, I finished reading the files that you shared with me from the data bank.",
                    "OptionText": null,
                    "NextItemIDList": [
                        200640001
                    ],
                    "Sender": "NPC"
                },
                "200640001": {
                    "Id": 200640001,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": null,
                    "Name": null,
                    "IconPath": null,
                    "MainText": "After sitting in this Seat of Divine Foresight for so long, I can't help but overlook the world outside of Luofu's delves.\\nThat, combined with the long lifespan of the Xianzhou humans, made me feel that the entire world is as boring and changeless as myself.\\nHowever, now I've read the wondrous encounters of the Express Crew, I realized that using my endless lifespan to travel various worlds would be a fun venture indeed.",
                    "OptionText": null,
                    "NextItemIDList": [
                        200640002
                    ],
                    "Sender": "NPC"
                },
                "200640002": {
                    "Id": 200640002,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": null,
                    "Name": null,
                    "IconPath": null,
                    "MainText": "Unfortunately... I still shoulder Luofu's countless duties and cannot accompany your group. But if I were to retire one day, what should I do?",
                    "OptionText": null,
                    "NextItemIDList": [
                        200640003,
                        200640005,
                        200640007
                    ],
                    "Sender": "NPC"
                },
                "200640003": {
                    "Id": 200640003,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": null,
                    "Name": null,
                    "IconPath": null,
                    "MainText": "Our Express is pretty big. You can come visit, general",
                    "OptionText": "Become a Nameless",
                    "NextItemIDList": [
                        200640004
                    ],
                    "Sender": "Player"
                },
                "200640005": {
                    "Id": 200640005,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": null,
                    "Name": null,
                    "IconPath": null,
                    "MainText": "Perhaps you can swap places with Fu Xuan and be the master diviner",
                    "OptionText": "Take Fu Xuan's job",
                    "NextItemIDList": [
                        200640006
                    ],
                    "Sender": "Player"
                },
                "200640007": {
                    "Id": 200640007,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": null,
                    "Name": null,
                    "IconPath": null,
                    "MainText": "I feel the Realm-Keeping Commission's work is quite relaxed...",
                    "OptionText": "Work for the Realm-Keeping Commission",
                    "NextItemIDList": [
                        200640008
                    ],
                    "Sender": "Player"
                },
                "200640004": {
                    "Id": 200640004,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": null,
                    "Name": null,
                    "IconPath": null,
                    "MainText": "Hahahaha, if that's a personal invitation, {NICKNAME}, then I might consider it.",
                    "OptionText": null,
                    "NextItemIDList": [
                        200640010
                    ],
                    "Sender": "NPC"
                },
                "200640006": {
                    "Id": 200640006,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": null,
                    "Name": null,
                    "IconPath": null,
                    "MainText": "Hahaha, don't ever mention that to me again. The master diviner is an exceedingly busy job. No ordinary person can face its arduous work.",
                    "OptionText": null,
                    "NextItemIDList": [
                        200640010
                    ],
                    "Sender": "NPC"
                },
                "200640008": {
                    "Id": 200640008,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": null,
                    "Name": null,
                    "IconPath": null,
                    "MainText": "Hahaha, that would truly take me back to my origins.",
                    "OptionText": null,
                    "NextItemIDList": [
                        200640009
                    ],
                    "Sender": "NPC"
                },
                "200640010": {
                    "Id": 200640010,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": null,
                    "Name": null,
                    "IconPath": null,
                    "MainText": "That reminds me... My dream as a young boy was to become a Galaxy Ranger.",
                    "OptionText": null,
                    "NextItemIDList": [
                        200640011
                    ],
                    "Sender": "NPC"
                },
                "200640009": {
                    "Id": 200640009,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": null,
                    "Name": null,
                    "IconPath": null,
                    "MainText": "My parents were scholars of the Realm-Keeping Commission and were responsible for researching and recording the histories of long-life species. The paperwork involved was literal torment.",
                    "OptionText": null,
                    "NextItemIDList": [
                        200640010
                    ],
                    "Sender": "NPC"
                },
                "200640011": {
                    "Id": 200640011,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": null,
                    "Name": null,
                    "IconPath": null,
                    "MainText": "Pity that life never goes the way you want it to go.",
                    "OptionText": null,
                    "NextItemIDList": [
                        200640012
                    ],
                    "Sender": "NPC"
                },
                "200640012": {
                    "Id": 200640012,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": null,
                    "Name": null,
                    "IconPath": null,
                    "MainText": "If given the chance, where would you want to go?",
                    "OptionText": "If given the chance...",
                    "NextItemIDList": [
                        200640013
                    ],
                    "Sender": "Player"
                },
                "200640013": {
                    "Id": 200640013,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": null,
                    "Name": null,
                    "IconPath": null,
                    "MainText": "Perhaps I'll go seek out animals that have already gone extinct in Xianzhou.",
                    "OptionText": null,
                    "NextItemIDList": [
                        200640014
                    ],
                    "Sender": "NPC"
                },
                "200640014": {
                    "Id": 200640014,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": null,
                    "Name": null,
                    "IconPath": null,
                    "MainText": "Let us stop for today. Qingzu just carried in another pile of documents for me. Just looking at them gives me a headache.",
                    "OptionText": null,
                    "NextItemIDList": [
                        200640015
                    ],
                    "Sender": "NPC"
                },
                "200640015": {
                    "Id": 200640015,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": null,
                    "Name": null,
                    "IconPath": null,
                    "MainText": "You're working hard, general!",
                    "OptionText": "You're working hard, general!",
