{
    "Id": 2005,
    "Name": "Fictitious Wordsmithing",
    "Buff": {
        "Name": "Devious Flattery",
        "Desc": "Accumulate the amount of DMG dealt by allies' follow-up attacks. When this DMG amount reaches <unbreak>100%</unbreak>, deal a set amount of DMG to all enemies.",
        "Param": [
            9,
            2.2000000001862645
        ]
    },
    "Option": [
        {
            "Name": "Examination",
            "Desc": "Whimsicality no longer requires accumulated DMG dealt by follow-up attack. Instead, it triggers after allies use <color=#f29e38ff><unbreak>#1[i]</unbreak></color> instances of follow-up attack and the DMG dealt by this effect increases by <color=#f29e38ff><unbreak>#2[i]%</unbreak></color>.",
            "Param": [
                3,
                0.6000000005587935
            ]
        },
        {
            "Name": "Extrapolation",
            "Desc": "When the Whimsicality effect is triggered, regenerates Energy for all allies equal to <color=#f29e38ff><unbreak>#1[i]%</unbreak></color> of their respective Max Energy. Energy recovered via this effect can exceed the ally's Max Energy.",
            "Param": [
                0.3500000003259629
            ]
        },
        {
            "Name": "Reductio ad Absurdum",
            "Desc": "Apart from follow-up attacks, all other kinds of DMG dealt by allies can also accumulate progress equal to <color=#f29e38ff><unbreak>#1[i]%</unbreak></color> of the original DMG amount.",
            "Param": [
                0.18000000016763806
            ]
        }
    ],
