                },
                "121050014": {
                    "Id": 121050014,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": 42,
                    "Name": "大毫",
                    "IconPath": "SpriteOutput/AvatarRoundIcon/UI_Message_Contacts_Dahao.png",
                    "MainText": "カンパニーに依頼して確認するにしても、時間がかかる。だから将軍様は、星穹列車に助力を求めるように勧めたんだ。具体的に言えば、一覧表の中に怪しい者がいないか確認してほしい",
                    "OptionText": null,
                    "NextItemIDList": [
                        121050015
                    ],
