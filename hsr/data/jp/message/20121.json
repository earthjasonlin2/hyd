                "201210005": {
                    "Id": 201210005,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": null,
                    "Name": null,
                    "IconPath": null,
                    "MainText": "占いの方法は人それぞれだけど、私の場合、記憶だけでもその人の未来は予測できるわ。",
                    "OptionText": null,
                    "NextItemIDList": [
                        201210006
                    ],
