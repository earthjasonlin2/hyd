                "201200015": {
                    "Id": 201200015,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": null,
                    "Name": null,
                    "IconPath": null,
                    "MainText": "だが相対性理論には反していない。なぜなら、それらは何の情報も伝えていないからだ",
                    "OptionText": null,
                    "NextItemIDList": [
                        201200016
                    ],
                    "Sender": "NPC"
