                "201200020": {
                    "Id": 201200020,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": null,
                    "Name": null,
                    "IconPath": null,
                    "MainText": "科学的問題を解決するには、紙、ペン、消しゴムが必要だ",
