                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": 1009,
                    "Name": "アスター",
                    "IconPath": "SpriteOutput/AvatarRoundIcon/Avatar/1009.png",
                    "MainText": "お金で解決できることなら問題にならないけど、今回は少し特殊な状況なのよね",
                    "OptionText": null,
                    "NextItemIDList": [
                        122130010
                    ],
                    "Sender": "NPC"
                },
