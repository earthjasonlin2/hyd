                    "OptionText": null,
                    "NextItemIDList": [
                        122130013
                    ],
                    "Sender": "NPC"
                },
                "122130013": {
                    "Id": 122130013,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": 1009,
                    "Name": "アスター",
                    "IconPath": "SpriteOutput/AvatarRoundIcon/Avatar/1009.png",
