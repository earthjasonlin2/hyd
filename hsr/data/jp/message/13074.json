                    "NextItemIDList": [
                        130740004
                    ],
                    "Sender": "PlayerAuto"
                },
                "130740007": {
                    "Id": 130740007,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": null,
