                },
                "101070004": {
                    "Id": 101070004,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": null,
                    "Name": null,
                    "IconPath": null,
                    "MainText": "いい{M#だろう}{F#よ}、年寄りと子供を愛護するのも{M#俺}{F#私}の美徳",
                    "OptionText": "いい{M#だろう}{F#よ}、年寄りと子供を愛護するのも{M#俺}{F#私}の美徳",
