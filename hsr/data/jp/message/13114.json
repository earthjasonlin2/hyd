                    "Id": 131140017,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": 1305,
                    "Name": "Dr.レイシオ",
                    "IconPath": "SpriteOutput/AvatarRoundIcon/Avatar/1305.png",
                    "MainText": "「パートナー」という言葉は、彼が僕を過大評価して使っているだけだ",
                    "OptionText": null,
                    "NextItemIDList": [
                        131140018,
                        131140020
                    ],
                    "Sender": "NPC"
                },
