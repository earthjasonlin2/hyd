                "131140007": {
                    "Id": 131140007,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": null,
                    "Name": null,
                    "IconPath": null,
                    "MainText": "ちなみに、これ全部読むのにどれくらいかかる{F#}{M#んだ}？",
                    "OptionText": null,
                    "NextItemIDList": [
                        131140008
                    ],
                    "Sender": "PlayerAuto"
