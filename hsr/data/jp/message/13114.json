                    "Id": 131140018,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": null,
                    "Name": null,
                    "IconPath": null,
                    "MainText": "スクリューガムにパートナーなんて呼ばれたら",
                    "OptionText": "さすが{F#だね}{M#だな}",
                    "NextItemIDList": [
                        131140019
                    ],
