                },
                "121000002": {
                    "Id": 121000002,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": 38,
                    "Name": "夕葵",
                    "IconPath": "SpriteOutput/AvatarRoundIcon/UI_Message_Contacts_Xikui.png",
                    "MainText": "しかし、あまりにもつまらないイベントなので申し込みが少なく…そこで御空様に指示を受けた私が、彼を手伝うことになったのです。どうか参加していただけないでしょうか？",
                    "OptionText": null,
                    "NextItemIDList": [
                        121000010
                    ],
                    "Sender": "NPC"
                },
                "121000009": {
                    "Id": 121000009,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": 38,
                    "Name": "夕葵",
                    "IconPath": "SpriteOutput/AvatarRoundIcon/UI_Message_Contacts_Xikui.png",
                    "MainText": "安心してください。もし手伝ってもらえるなら私が給金を出しますので、損をすることはありません",
                    "OptionText": null,
                    "NextItemIDList": [
                        121000001
                    ],
                    "Sender": "NPC"
                },
