                },
                "121510109": {
                    "Id": 121510109,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": 1204,
                    "Name": "景元",
                    "IconPath": "SpriteOutput/AvatarRoundIcon/Avatar/1204.png",
                    "MainText": "もしそれらしきものを受け取った時は、速やかに神策府の青鏃に渡してくれ。そうすれば、彼女が十王司の専任者に小包を送り、処理するよう手配してくれる",
                    "OptionText": null,
                    "NextItemIDList": [
                        121510110
                    ],
                    "Sender": "NPC"
                },
