                "111160011": {
                    "Id": 111160011,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": null,
                    "Name": null,
                    "IconPath": null,
                    "MainText": "네가 보기에 그 친구는 어떤 사람이야?",
