                },
                "101250005": {
                    "Id": 101250005,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": null,
                    "Name": null,
                    "IconPath": null,
                    "MainText": "세차 행성 직원들과 열차 안팎 모두 깨끗하게 청소했죠……",
                    "OptionText": null,
