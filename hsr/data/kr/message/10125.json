                },
                "101250025": {
                    "Id": 101250025,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": null,
                    "Name": null,
                    "IconPath": null,
                    "MainText": "그는 아무래도 당신에게 감동을 받은 것 같네요……",
                    "OptionText": null,
                    "NextItemIDList": [
                        101250026
                    ],
