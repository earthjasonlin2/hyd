                    "ContactsID": 1305,
                    "Name": "Dr. 레이시오",
                    "IconPath": "SpriteOutput/AvatarRoundIcon/Avatar/1305.png",
                    "MainText": "보수적으로 계산하면 1 앰버기원이면 충분해",
                    "OptionText": null,
                    "NextItemIDList": [
                        131140011
                    ],
                    "Sender": "NPC"
                },
