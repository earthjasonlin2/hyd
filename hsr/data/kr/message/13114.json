                },
                "131140005": {
                    "Id": 131140005,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": null,
                    "Name": null,
                    "IconPath": null,
                    "MainText": "전 그냥 테스트 인원일 뿐 머리 쓰는 건 제 일이 아니에요",
                    "OptionText": "전 테스트만 해요",
                    "NextItemIDList": [
                        131140011
                    ],
                    "Sender": "Player"
                },
                "131140006": {
                    "Id": 131140006,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": null,
                    "Name": null,
                    "IconPath": null,
                    "MainText": "바로 배울게요, 배우는 걸 가장 좋아하거든요",
                    "OptionText": "바로 배울게요",
                    "NextItemIDList": [
                        131140007
                    ],
                    "Sender": "Player"
                },
                "131140009": {
                    "Id": 131140009,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": null,
                    "Name": null,
                    "IconPath": null,
                    "MainText": "흠, 저희가 아는 사이인가요?",
                    "OptionText": "누구시죠?",
                    "NextItemIDList": [
                        131140010
                    ],
                    "Sender": "Player"
