                "200690007": {
                    "Id": 200690007,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": null,
                    "Name": null,
                    "IconPath": null,
                    "MainText": "그럼 ≪에이스 비행≫을 추천해",
                    "OptionText": null,
                    "NextItemIDList": [
                        200690008
                    ],
                    "Sender": "NPC"
