                "200690011": {
                    "Id": 200690011,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": null,
                    "Name": null,
                    "IconPath": null,
                    "MainText": "천박사와 함대 경영을 체험해 보죠. 지금의 어공 씨처럼",
                    "OptionText": null,
