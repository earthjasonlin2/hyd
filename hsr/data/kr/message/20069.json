                "200690012": {
                    "Id": 200690012,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": null,
                    "Name": null,
                    "IconPath": null,
                    "MainText": "그럼 ≪천박사 조타수≫를 추천해",
                    "OptionText": null,
                    "NextItemIDList": [
                        200690013
                    ],
