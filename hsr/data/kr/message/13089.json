                "130890002": {
                    "Id": 130890002,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": null,
                    "Name": null,
                    "IconPath": null,
                    "MainText": "제 친구 중에 왕눈이는 없는데, 누구세요?",
                    "OptionText": "누구세요?",
                    "NextItemIDList": [
