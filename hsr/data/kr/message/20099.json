                "200990007": {
                    "Id": 200990007,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": null,
                    "Name": null,
                    "IconPath": null,
                    "MainText": "괜찮아요. 그런 일반인들의 관점이 필요해요",
