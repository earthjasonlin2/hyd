                },
                "201310016": {
                    "Id": 201310016,
                    "ItemType": "Text",
                    "ItemContentID": null,
                    "ContactsID": null,
                    "Name": null,
                    "IconPath": null,
                    "MainText": "뭐, 어쩔 수 없지",
                    "OptionText": null,
                    "NextItemIDList": [
                        201310017
                    ],
